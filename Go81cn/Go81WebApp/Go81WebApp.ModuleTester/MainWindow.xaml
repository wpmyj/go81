    <Window.Resources>
        <DataTemplate x:Key="DatePickTemplate">
            <DatePicker Text="{Binding d, StringFormat={}{0:yyyy-MM-dd}}"/>
        </DataTemplate>
    </Window.Resources>
    <Grid>
        <Button Content="功能测试" HorizontalAlignment="Left" Height="30" Margin="10,10,0,0" VerticalAlignment="Top" Width="80" Click="Button_Click1"/>
        <Button Content="功能测试2" HorizontalAlignment="Left" Height="30" Margin="10,45,0,0" VerticalAlignment="Top" Width="80" Click="Button_Click2"/>
        <Button Content="功能测试3" HorizontalAlignment="Left" Height="30" Margin="10,80,0,0" VerticalAlignment="Top" Width="80" Click="Button_Click3"/>
        <Button Content="功能测试4" HorizontalAlignment="Left" Height="30" Margin="10,115,0,0" VerticalAlignment="Top" Width="80" Click="Button_Click4"/>
        <Button Content="功能测试5" HorizontalAlignment="Left" Height="30" Margin="10,150,0,0" VerticalAlignment="Top" Width="80" Click="Button_Click5"/>
        <Button Content="功能测试6" HorizontalAlignment="Left" Height="30" Margin="10,185,0,0" VerticalAlignment="Top" Width="80" Click="Button_Click6"/>
