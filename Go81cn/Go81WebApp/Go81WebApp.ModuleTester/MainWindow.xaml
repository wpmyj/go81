        <TabControl Margin="95,10,10,10">
            <TabItem Header="TabItem">
                <Grid>
                    <TextBox x:Name="textBox1" Margin="10" TextWrapping="Wrap"/>
                </Grid>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid>
                    <Image x:Name="img1" Margin="10"/>
                </Grid>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Button Grid.Row="0" Content="Button31" HorizontalAlignment="Left" Margin="8,8,0,0" VerticalAlignment="Top" Padding="8,0"  Height="24" Click="Button31_OnClick"/>
                    <Button Grid.Row="0" Content="Button32" HorizontalAlignment="Left" Margin="88,8,0,0" VerticalAlignment="Top" Padding="8,0"  Height="24" Click="Button32_OnClick"/>
                    <Button Grid.Row="0" Content="Button33" HorizontalAlignment="Left" Margin="168,8,0,0" VerticalAlignment="Top" Padding="8,0"  Height="24" Click="Button33_OnClick"/>
                    <Button Grid.Row="0" Content="Button34" HorizontalAlignment="Left" Margin="248,8,0,0" VerticalAlignment="Top" Padding="8,0"  Height="24" Click="Button34_OnClick"/>
                    <DataGrid x:Name="dataGrid1" Grid.Row="1"
                              AutoGeneratingColumn="DataGrid1_OnAutoGeneratingColumn"
                              AutoGeneratedColumns="DataGrid1_OnAutoGeneratedColumns"
                              />
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
