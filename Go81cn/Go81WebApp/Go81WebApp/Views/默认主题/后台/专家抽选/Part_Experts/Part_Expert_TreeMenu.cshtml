@model Go81WebApp.Models.数据模型.用户数据模型.单位用户
<link rel="stylesheet" type="text/css" href="~/css/gys_treemenu.css" />
<link rel="stylesheet" type="text/css" href="~/css/index.css" />
<script type="text/javascript" src="~/JS/jquery-1.6.min.js"></script>
<script type="text/javascript" src="~/JS/SimpleTree.js"></script>
<script type="text/javascript">
    $(function () {
        $(".st_tree").SimpleTree({
            click: function (a) {
                if (!$(a).attr("hasChild")) {
                    window.location.href = $(a).attr("href");
                }
            }
        });
    });
</script>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <div class="index_menu_top">
        <div class="index_menu_top_middle">
            <div class="index_menu_top_title">栏目分类</div>
        </div>
    </div>
    <div class="st_tree">
        <ul>
            <!--<li><a href="#" ref="hyjm">欢迎界面</a></li>-->
            <li class="st_tree_first_tittle"><a href="#">消息管理</a></li>
            <ul show="true">
                <li class="st_tree_second_title"><a href="#">站内消息</a></li>
                <ul show="true">
                    <li class="st_tree_third_title"><a href="~/单位用户后台/Procure_ZnxxAdd">发新消息</a></li>
                    <li class="st_tree_third_title"><a href="~/单位用户后台/Procure_Znxx">已收消息</a></li>
                    <li class="st_tree_third_title"><a href="~/单位用户后台/Procure_ZnxxSend">已发消息</a></li>
                </ul>
            </ul>
            <li class="st_tree_first_tittle"><a href="#">公告管理</a></li>
            <ul show="true">
                <li class="st_tree_second_title"><a href="~/单位用户后台/Procure_AdList">公告列表</a></li>
