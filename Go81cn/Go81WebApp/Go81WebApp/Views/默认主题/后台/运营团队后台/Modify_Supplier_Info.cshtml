                        <select id="firstClass" onchange="searchtype(this)">
                            @foreach (var item in ViewData["行业列表"] as IEnumerable<商品分类>)
                            {
                                if (item.分类性质.ToString() != "专用物资")
                                {
                            <option value="@item.分类名">@item.分类名</option>
                                }
                            }
                        </select>
                    </form>
                    <div style=" width:100%;">
                        <p>二级分类名：</p><table width="100%" id="thirdType"></table>
                    </div>
                    <div style="text-align: center;">
                        <input type="button" disabled="disabled" onclick="sendGoodType()" id="bt" style="background-color:gray;border:none; color:white; width:50px; height:30px;" value="添加">

                        <span style="display:block;" id="warn"></span>
                    </div>
                </div>

            </div>
            <div style="clear:both;"></div>
        </div>
    </div>
</div>
<div id="modifyProduct" style="display:none;">
