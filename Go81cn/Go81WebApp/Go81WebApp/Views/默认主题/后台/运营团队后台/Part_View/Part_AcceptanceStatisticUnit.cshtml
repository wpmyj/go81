
@{
    var ysd_unit = ViewData["年度统计"] as IOrderedEnumerable<Tuple<string, decimal>>;
}

<table class="gys_statistic" cellpadding="0" cellspacing="0" style="border: 1px solid #08A86B;">
    @if (ViewData["年份"] != "0")
    {
        <tr><th colspan="2" style="line-height: 38px;background: #08A86B;color:#F5E9F0">单位用户 @ViewData["年份"] 年成交总金额</th></tr>
    }
    else
    {
        <tr><th colspan="2" style="line-height: 38px;background: #08A86B;color:#F5E9F0">单位用户历年成交总金额</th></tr>
    }
    @foreach (var k in ysd_unit)
    {
        <tr>
            <td width="30%">@k.Item1</td>
            <td>@k.Item2.ToString("C")</td>
        </tr>
    }
</table>





