@model Go81WebApp.Models.数据模型.消息数据模型.投诉
<div class="title-base-out">
    <div class="title-base-middle">
        <div class="title-base-inner">投诉详情</div>
    </div>
</div>
<div class="gys_znxx_box">
    <div class="gys_znxx_contentbox">
        <div style="width:700px;  margin-top:20px;font-size:14px; margin-bottom:20px; background-color:rgb(242, 247, 244); word-break:break-all; height:auto; margin-left:20px; font-size:14px;">
            <table width="100%" cellpadding="0" cellspacing="0">
                @foreach (var item in Model.对话消息)
                {
                    <tr><td align="left" valign="top" style="margin-bottom:10px;"><strong>标题：</strong><input type="text" style="width:500px;" value="@item.消息主体.标题" /></td></tr>
                    <tr><td align="left" valign="top" style="margin-bottom:10px;"><strong>投诉人：</strong>@Model.发起者.用户数据.登录信息.登录名</td></tr>
                    <tr><td align="left" valign="top" style="margin-bottom:10px;"><strong>内容：</strong>@Html.Raw(item.消息主体.内容)</td></tr>
                    <tr>
                        <td align="center">
                            <a href="/运营团队后台/Suggestion" style="display:block;width:80px; height:40px;color:white; text-align:center; line-height:40px; background-color:rgb(172, 177, 170);">返回</a>
                        </td>
                    </tr>
                    break;
                }
            </table>
        </div>
        </div>
    </div>
<script type="text/javascript">
    $(function () {
        $("#已处理投诉").addClass("left_menu_select").children("a").css({ "color": "#fff" });
        $("#已处理投诉").parents().attr("show", "true").show().prev("li").addClass("open").children("span").css({ "background": "url('../Images/tree_icons.png') -115px -4px" });
    });
</script>

