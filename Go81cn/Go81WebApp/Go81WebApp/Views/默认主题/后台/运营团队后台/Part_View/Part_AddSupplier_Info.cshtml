                                        <div class="arrow"></div>
                                        <div class="con_box">此处是贵企业的认证级别</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.供应商用户信息.认证级别)
                                </td>
                            </tr>
                            <tr>
                                <td align="right" class="lab">所属行业：</td>
                                <td>
                                    @Html.HiddenFor(m => m.企业基本信息.所属行业, new { id = "hidden" })@Html.ValidationMessageFor(m => m.企业基本信息.所属行业)
                                    @{
    IEnumerable<商品分类> goodclass = ViewData["行业列表"] as IEnumerable<商品分类>;
    if (goodclass.Count() != 0)
    {
                                <select class="industry" style="width:153px; margin-left:2px;">
                                    <option>请选择行业</option>
                                    @foreach (var i in goodclass)
            {
                <option class="industry" id="@i.Id">@i.分类名</option>
            }
                                </select>
    }
                                    }
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请选择贵企业所属行业,如零售业</div>
                                    </div>
                                </td>
                                <td align="right" class="lab">所属地域：</td>
                                <td>
                                    @Html.HiddenFor(m => m.所属地域.省份, new { id = "provence" })
                                    @Html.HiddenFor(m => m.所属地域.城市, new { id = "city" })
                                    @Html.HiddenFor(m => m.所属地域.区县, new { id = "area" })
                                    <select id="deliverprovince" name="deliverprovince" style="width:153px; margin-left:2px;"></select>
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请选择贵企业所在省份</div>
                                    </div>
                                    <br />
                                    <select id="delivercity" name="delivercity" style="width:153px; margin-left:2px;"></select>
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请选择贵企业所在城市</div>
                                    </div>
                                    <br />
                                    <select id="deliverarea" name="deliverarea" style="width:153px; margin-left:2px;"></select>
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请选择贵企业所在区县</div>
                                    </div>
                                    <script>
                                    window.onload = function () { new PCAS("deliverprovince", "delivercity", "deliverarea"); }
                                    </script>
                                </td>

                            </tr>
                            <tr>
                                <td align="right" class="lab">经营类型：</td>
                                <td>
                                    @Html.EnumDropDownListFor(m => m.企业基本信息.经营类型, new { @style = "width:153px; margin-left:2px;" })
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请选择企业经营类型</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业基本信息.经营类型)
                                </td>
                                <td align="right" class="lab">经营子类型：</td>
                                <td>
                                    @Html.EnumDropDownListFor(m => m.企业基本信息.经营子类型, new { @style = "width:153px; margin-left:2px;" })
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请选择经营子类型</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业基本信息.经营子类型)
                                </td>
                            </tr>
                            <tr>
                                <td align="right" class="lab">员工人数：</td>
                                <td>
                                    @Html.EnumDropDownListFor(m => m.企业基本信息.员工人数, new { @style = "width:153px; margin-left:2px;" })
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请选择员工人数</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业基本信息.员工人数)
                                </td>
                                <td align="right" class="lab">经济性质：</td>
                                <td>
                                    @Html.EnumDropDownListFor(m => m.企业基本信息.经济性质, new { @style = "width:153px; margin-left:2px;" })
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请选择贵企业的经济性质</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业基本信息.经济性质)
                                </td>
                            </tr>
                            <tr>
                                <td align="right" class="lab">网站地址：</td>
                                <td>
                                    @Html.TextBoxFor(m => m.企业基本信息.网站网址)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请选择贵企业网址</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业基本信息.网站网址)
                                </td>
                                <td align="right" class="lab">供应商名称：</td>
                                <td>
                                    @Html.TextBoxFor(m => m.供应商用户信息.供应商显示名)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请选择供应商名称，如：未来科技有限企业</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.供应商用户信息.供应商显示名)
                                </td>
                            </tr>
                            <tr>
                                <td align="right">展示图片：</td>
                                <td>
                                    <input type="file" name="file" style="width:130px; height:30px;" />
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请上传企业电子原件扫描件图片，本图片用于网站首页展示。<em style="color:red;">注：请上传2M以下的jpg格式的彩色扫描件图片</em></div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td align="right" class="lab" valign="top">企业简介：</td>
                                <td align="left" colspan="3">
                                    @Html.TextAreaFor(m => m.企业基本信息.企业简介, new { cols = "80", rows = "5" })
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请对贵企业做简单介绍</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业基本信息.企业简介)
                                </td>
                            </tr>
                            <tr>
                                <td align="right" class="lab" valign="top">参加投标经历：</td>
                                <td colspan="3">
                                    @Html.TextAreaFor(m => m.企业基本信息.参加投标经历, new { rows = "5", cols = "80" })
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写贵企业近年的投标经历</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业基本信息.参加投标经历)
                                </td>
                            </tr>
                        </table>
                        </div>
                    </div>
                <div>
                    <div id="title_div" class="other_title title_addsupplier">企业联系方式</div>
                    <div class="gys_znxx_box" style="width:95%; margin-left:auto; margin-right:auto;">
                        <table width="80%" style="margin-left:50px;">
                            <tr>
                                <td width="25%" align="right" class="lab">联系人姓名：</td>
                                <td width="25%">
                                    @Html.TextBoxFor(m => m.企业联系人信息.联系人姓名)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写联系人姓名,如张晓东</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业联系人信息.联系人姓名)
                                </td>
                                <td width="25%" align="right" class="lab">联系人手机：</td>
                                <td width="25%">
                                    @Html.TextBoxFor(m => m.企业联系人信息.联系人手机)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写联系人手机号码,如18280465879,15112567894</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业联系人信息.联系人手机)
                                </td>
                            </tr>
                            <tr>
                                <td align="right" class="lab">联系人邮箱：</td>
                                <td>
                                    @Html.TextBoxFor(m => m.企业联系人信息.联系人邮箱)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写联系人邮箱，可以是QQ邮箱，如：1487589621@qq.com</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业联系人信息.联系人邮箱)
                                </td>
                                <td align="right" class="lab">联系人固定电话：</td>
                                <td>
                                    @Html.TextBoxFor(m => m.企业联系人信息.联系人固定电话)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写联系人固定电话,格式为028-54892547或者0816-4587456</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业联系人信息.联系人固定电话)
                                </td>
                            </tr>
                            <tr>
                                <td align="right" class="lab">联系人传真：</td>
                                <td>
                                    @Html.TextBoxFor(m => m.企业联系人信息.联系人传真)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写联系人的传真，格式：028-45628976或者0816-4586579</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业联系人信息.联系人传真)
                                </td>
                                <td align="right" class="lab">联系人地址：</td>
                                <td>
                                    @Html.TextBoxFor(m => m.企业联系人信息.联系人地址)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写联系人的联系地址</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业联系人信息.联系人地址)
                                </td>
                            </tr>
                        </table>
                        </div>
                    </div>
                <div id="title_div" class="other_title title_addsupplier">参标经历<a href="###" onclick="addtoubiao(this)">【新增】</a></div>
                <div class="gys_znxx_box" style="width:95%; margin-left:auto; margin-right:auto;">
                    <div id="toubiao" style="border-color:rgb(207, 197, 197);  margin-bottom:10px; margin-top:10px;">
                        <script>
                            var s = '<table width="80%" style="margin-left:35px;"><tr><td width="25%" align="right">项目名称：</td>' +
                                '<td width="25%"><input  onmouseover="show_tip(this)" onmouseout="hide_tip(this)" type="text" name="历史参标记录[`].项目名称"/><div class="out_box"><div class="arrow"></div><div class="con_box">请填写项目名称</div></div></td>' +
                                '<td width="25%" align="right">招标方：</td><td width="25%"><input  onmouseover="show_tip(this)" onmouseout="hide_tip(this)" type="text" name="历史参标记录[`].招标方"/><div class="out_box"><div class="arrow"></div><div class="con_box">请填写招标方</div></div></td>' +
                                '</tr><tr><td  align="right">参标结果：</td><td><select style="width:153px; margin-left:2px;"  onmouseover="show_tip(this)" onmouseout="hide_tip(this)" name="历史参标记录[`].参标结果"><option value="0">请选择参标结果</option><option value="1">中标</option><option value="2">落标</option><option value="3">流标</option><option value="4">废标</option><option value="9999">其他</option></select><div class="out_box"><div class="arrow"></div><div class="con_box">请选择参标结果</div></div></td>' +
                                '<td  align="right">参标时间：</td><td><input onmouseover="show_tip(this)" onmouseout="hide_tip(this)" onfocus="WdatePicker({isShowWeek:true})" type="text" name="历史参标记录[`].参标时间"/><div class="out_box"><div class="arrow"></div><div class="con_box">请填写参标时间</div></div></td></tr>' +
                                '<tr><td align="right"><input  onmouseover="show_tip(this)" onmouseout="hide_tip(this)" value="true" type="checkbox" name="历史参标记录[`].本平台项目"/><div class="out_box"><div class="arrow"></div><div class="con_box">请确认是否是本平台项目</div></div></td><td>是否是本平添项目</td></tr></table><hr />';
                            var c = @Model.历史参标记录.Count;
                            function addtoubiao(th) {
                                $(th).remove();
                                $('#toubiao').append(s.replace(/`/g, c++));
                                $('#toubiao h1').hide();
                            }
                        </script>
                        @if (Model.历史参标记录 != null && Model.历史参标记录.Count() != 0)
                        {
                            for (int i = 0; i < Model.历史参标记录.Count; i++)
                            {
                        <table width="80%" style="margin-left:35px;">
                            <tr>
                                <td width="25%" align="right">项目名称：</td>
                                <td width="25%">
                                    @Html.TextBoxFor(m => m.历史参标记录[i].项目名称)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写项目名称</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.历史参标记录[i].项目名称)
                                </td>
                                <td width="25%" align="right" class="lab">招标方：</td>
                                <td width="25%">
                                    @Html.TextBoxFor(m => m.历史参标记录[i].招标方)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写招标方</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.历史参标记录[i].招标方)
                                </td>
                            </tr>
                            <tr>
                                <td align="right" class="lab">招标结果：</td>
                                <td>
                                    @Html.EnumDropDownListFor(m => m.历史参标记录[i].参标结果)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请选择参标结果</div>
                                    </div>
                                    <br />@Html.ValidationMessageFor(m => m.历史参标记录[i].参标结果)
                                </td>
                                <td align="right" class="lab">
                                    @Html.EditorFor(m => m.历史参标记录[i].本平台项目)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请确认是否是本平台项目</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.历史参标记录[i].本平台项目)
                                </td>
                                <td>
                                    请确认是否是本平台项目
                                </td>
                            </tr>
                            <tr>
                                <td align="right" class="lab">参标时间：</td>
                                <td>
                                    @Html.TextBoxFor(m => m.历史参标记录[i].参标时间)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写参标时间</div>
                                    </div>
