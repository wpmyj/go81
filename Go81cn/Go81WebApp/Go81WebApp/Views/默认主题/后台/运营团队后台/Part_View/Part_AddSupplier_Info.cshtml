@model Go81WebApp.Models.数据模型.用户数据模型.供应商
@using Go81WebApp.Models.数据模型.商品数据模型
<link href="~/css/css.css" rel=" stylesheet" />
<link href="~/css/index.css" rel="stylesheet" />
<link href="~/css/Msg_Tips.css" rel="stylesheet" />
<script src="~/JS/provinceandcity_all.js"></script>
<script type="text/javascript" src="~/JS/My97DatePicker/calendar.js"></script>
<script type="text/javascript" src="~/JS/My97DatePicker/config.js"></script>
<script type="text/javascript" src="~/JS/My97DatePicker/WdatePicker.js"></script>
<style>
    table tr td span {
        font-size: 12px;
        color: red;
    }

    table tr td {
        font-size: 14px;
    }

    .list {
        font-size: 14px;
        width: 1003px;
        background-image: url(/images/index_content_bg.jpg);
    }

        .list a {
            width: 100px;
            cursor: pointer;
            height: 30px;
            color: #336;
            display: inline-block;
            text-align: center;
            line-height: 30px;
        }
</style>
<div class="title-base-out">
    <div class="title-base-middle">
        <div class="title-base-inner">添加供应商</div>
    </div>
</div>
@using (Html.BeginForm("AddSupplier", "运营团队后台", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.HiddenFor(m => m.Id)
    <div class="gys_znxx_box">
        <div style="width:920px;overflow: hidden;margin:20px auto;">
            <div class="list">
                <a onclick="checks(this,'.Fifth_Page')">登录信息</a><a onclick="checks(this, '.First_Page')">企业基本信息</a><a onclick="checks(this, '.Second_Page')">账号基本信息</a><a onclick="checks(this, '.Third_Page')">营业信息</a><a onclick="checks(this, '.Forth_Page')">财务税务信息</a>
            </div>
            <div class="Fifth_Page" style="margin-left:0px;border-color:rgb(207, 197, 197); margin-top:0px; border-style:solid;border-width:1px;">
                <div id="title_div" class="other_title title_addsupplier">登录信息</div>
                <div class="gys_znxx_box" style="width:95%; margin-bottom:10px; margin-left:auto; margin-right:auto;">
                    <table width="90%" style="margin-left:20px;">
                        <tr>
                            <td width="15%" align="right">登录名：</td>
                            <td width="50%">
                                @Html.TextBoxFor(m => m.登录信息.登录名, new { id = "loginName" })
                                <div class="out_box">
                                    <div class="arrow"></div>
                                    <div class="con_box">请填写登录名,如:张珊,Tom,Marry等</div>
                                </div>
                                <span id="warn"></span>
                                @Html.ValidationMessageFor(m => m.登录信息.登录名)
                            </td>
                        </tr>
                        <tr>
                            <td align="right">登录密码：</td>
                            <td>
                                @Html.PasswordFor(m => m.登录信息.密码)
                                <div class="out_box">
                                    <div class="arrow"></div>
                                    <div class="con_box">请填写用户登录的初始密码,如:123456xy</div>
                                </div>
                                @Html.ValidationMessageFor(m => m.登录信息.密码)
                            </td>
                        </tr>
                    </table>
                </div>
                </div>
            <div class="First_Page" style="margin-left:0px; border:1px solid rgb(202, 195, 195); display:none; margin-top:0px;">
                <div>
                    <div id="title_div" class="other_title title_addsupplier">企业基本信息管理</div>
                    <div class="gys_znxx_box" style="width:95%; margin-left:auto; margin-right:auto;">
                        <table width="90%" style="margin-left:30px;">
                            <tr>
                                <td width="15%" align="right" class="lab">企业名称：</td>
                                <td width="35%">
                                    @Html.TextBoxFor(m => m.企业基本信息.企业名称)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写企业名称,如:未来科技有限企业</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业基本信息.企业名称)
                                </td>
                                <td width="15%" align="right" class="lab">注册地址：</td>
                                <td width="35%">
                                    @Html.TextBoxFor(m => m.企业基本信息.注册地址)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写注册地址,如:四川省成都市工商局</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业基本信息.注册地址)
                                </td>
                            </tr>
                            <tr>
                                <td align="right" class="lab">成立时间：</td>
                                <td>
                                    @Html.TextBoxFor(m => m.企业基本信息.成立时间, new { onfocus = "WdatePicker({isShowWeek:true})" })
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写企业成立时间,如:2012/02/02</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业基本信息.成立时间)
                                </td>
                                <td align="right" class="lab">邮政编码：</td>
                                <td>
                                    @Html.TextBoxFor(m => m.企业基本信息.邮政编码)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写企业所在地的邮编号码,如:614002</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.企业基本信息.邮政编码)
                                </td>
                            </tr>
                            <tr>
                                <td align="right" class="lab">供应商编码：</td>
                                <td>
                                    @Html.EditorFor(m => m.供应商用户信息.供应商编码)
                                    <div class="out_box">
                                        <div class="arrow"></div>
                                        <div class="con_box">请填写供应商编码</div>
                                    </div>
                                    @Html.ValidationMessageFor(m => m.供应商用户信息.供应商编码)
                                </td>
                                <td align="right" class="lab">认证级别：</td>
                                <td>
                                    @Html.TextBoxFor(m => m.供应商用户信息.认证级别, new { @readonly = "readonly" })
