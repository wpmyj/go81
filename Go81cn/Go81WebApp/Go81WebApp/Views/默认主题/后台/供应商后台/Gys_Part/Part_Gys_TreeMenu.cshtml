    <script type="text/javascript">
        $(function () {
            $(".st_tree").SimpleTree({
                click: function (a) {
                    if (!$(a).attr("hasChild")) {
                        window.location.href = $(a).attr("href");
                    }
                }
            });

            if ($("#links").val() != null && $("#links").val() != "") {
                var dom = $("#" + $("#links").val());
                $(dom).css({ "background": "#537E2B" }).children("a").css({ "color": "#fff" });
            }
        });


    </script>


