<script>
    function ch_box(num, str)
    {
            $(".tb span").each(function () {
                $(this).css("background-color", "orange");
            }); $(".tb div").each(function () {
                $(this).hide();
            });
            $(".tb span#sp" + num).css("background-color", "red");
            $(".tb " + str).show();
 }
</script>

