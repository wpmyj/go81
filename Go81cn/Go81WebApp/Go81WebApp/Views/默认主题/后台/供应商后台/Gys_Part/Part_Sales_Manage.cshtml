<script type="text/javascript" language="javascript">
    function showButton(th, str) {
        $(":file[name='" + str + "']").show();
        $(th).hide();
    }
    function SubmitForm() {
        $("form[name='SalesData']").submit();
    }
    function showPic(th) {
        $("#picture img").attr("src", th.title);
        $("#picture").css("visibility", "visible");
    }
    function hidePic() {
        $("#picture").css("visibility", "hidden");
    }
    $(document).ready(function () {
        $("a.modify").click(function () { $(".company_msg :text,.company_msg select,.company_msg textarea,.company_msg :checkbox,.company_msg,:file,#submit_btn").each(function () { $(this).removeAttr("disabled"); $(".company_msg a.add").show(); }); $("#submit_btn").css("background-color", "#2c5735"); $("input[type='file']").each(function () { $(this).removeAttr("disabled"); }); });
        $("table tr td :input").mouseover(function () {
            $(this).next("div").show();
            $(this).next("div").css("display", "inline-block");
        });
        $("table tr td :input").mouseout(function () { $(this).next("div").hide(); });
        $("#营业信息").css({ "background": "#537E2B" }).children("a").css({ "color": "#fff" });
        $("#营业信息").parents().attr("show", "true").show().prev("li").addClass("open").children("span").css({ "background": "url('../Images/tree_icons.png') -115px -4px" });
    });
    $("#deliverprovince").blur(function () {
        var val = $("select#deliverprovince").val();
        $("#provence").val(val);
    });
    $("#delivercity").blur(function () {
        var val = $("select#delivercity").val();
        $("#city").val(val);
    });
    $("#deliverarea").blur(function () {
        var val = $("select#deliverarea").val();
        $("#area").val(val);
    });
    function province(th) {
        $("#provence").val($(th).val());
    }
    function setCity(th) {
        $("#city").val($(th).val());
    }
    function setArea(th) {
        $("#area").val($(th).val());
    }
    function setCatorgray(th) {
        $("#catogray").val($(th).val());
    }
    function show_tip(t) {
        $(t).next("div").show();
        $(t).next("div").css("display", "inline-block");
    }
    function hide_tip(t) {
        $(t).next("div").hide();
    }
    function showdiv(th) {
        $(th).parent().children("div").show();
        $(th).parent().children("div").css("display", "inline-block");
    }
    function hidediv(th) {
        $(th).parent().children("div").hide();
    }
    function settype(th) {
        if (th.value == "1") {
            $(":text#endtime").attr("disabled", "disabled");
            $(":text#endtime").val("0001/1/1 0:00:00");
        }
        else {
            $(":text#endtime").removeAttr("disabled");
        }
    }
    function fileChange(target, str1, str2) {
        $(str1).children("#framebox").show();
        document.forms[str2].submit();
        //$("#f").show();
        $("#f1").show();
        $(target).prev("span").hide();
    }
    function hideTip(th, tip) {
        $(tip).hide();
    }
    function showTip(th, tip) {
        if (th.value == "") {
            $(tip).show();
        }
        else {
            if (th.value.length > 100) {
                $(tip).show();
                $(th).blur();
            }
            else if (th.value.length > 0 && th.value.length<=100)
            {
                $(tip).hide();
            }
        }
    }
    function showOrHide(th, tip) {
        if (th.value.length > 100) {
            $(tip).show();
            $(th).blur();
        }
        else {
            $(tip).hide();
        }
    }
</script>
<script type="text/javascript" language="javascript">
    $(".company_msg a.add").hide();
    $(".company_msg :text,.company_msg select,.company_msg textarea,.company_msg :checkbox,.company_msg,:file,:input#submit_btn").each(function () { $(this).attr("disabled", "disabled"); $(this).css("background-color", "white"); });
</script>