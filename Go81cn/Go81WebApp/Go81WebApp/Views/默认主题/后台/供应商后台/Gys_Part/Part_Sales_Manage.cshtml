@model Go81WebApp.Models.数据模型.用户数据模型.供应商
<script type="text/javascript" src="~/JS/My97DatePicker/calendar.js"></script>
<script type="text/javascript" src="~/JS/My97DatePicker/config.js"></script>
<script type="text/javascript" src="~/JS/My97DatePicker/WdatePicker.js"></script>
<script src="~/JS/provinceandcity_all.js"></script>
<link href="~/css/Msg_Tips.css" rel="stylesheet" />
<link href="~/css/css.css" rel=" stylesheet" />
<style>
    table tr td span {
        background-color: #ffebeb;
        font-size: 12px;
        color: red;
    }

    table tr td span {
        color: red;
        font-size: 12px;
    }

    td {
        position: relative;
    }

    div.out_box {
        width: 135px;
        left: initial;
    }
    .bg_outdiv
    {
        overflow:visible;
    }
</style>
<div id="f1" class="modal_out">
    <div class="modal_in"></div>
    <div class="modal_content_loading">
        <img src="~/Images/ajax-loader.gif" />
        <span>正在上传，请稍等...</span>
    </div>
</div>
<div id="picture" style="position:absolute; text-align:center; width:auto; height:auto; background-color:green; box-shadow:0px 0px 10px black;-webkit-box-shadow:0px 0px 10px black;-moz-box-shadow:0px 0px 10px black; margin:50px auto 0px auto;z-index:100;visibility:hidden;">
    <div style="width:100%; text-align:right;">
        <a style="cursor:pointer;  background-color:white; width:50px; height:25px;" onclick="hidePic()">关闭</a>
    </div>
    <img src="" />
</div>
<div id="addContent"></div>
<div style="font-size:14px;">
    <div id="title_div" class="gys_xttz_topbg">
        <div class="index_bottom_zb_top_title">
            营业信息
            @if (Model.供应商用户信息.已提交)
            {
                <span style="font-size:15px; color:red;"><em>您已提交过申请，不能修改信息。如需修改信息，请联系网站管理员。</em></span>
            }
        </div>
    </div>
    @using (Html.BeginForm("Sales_Manage", "供应商后台", FormMethod.Post, new { enctype = "multipart/form-data", name = "SalesData" }))
    {   @Html.HiddenFor(m => m.Id)
        <div class="company_msg">
            <div class="gys_znxx_box">
                <table width="95%" cellspacing="0">
                    <tr>
                        <td width="20%" align="right">组织机构代码：</td>
                        <td width="30%">
                            @Html.TextBoxFor(m => m.工商注册信息.组织机构代码)
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">请填写组织机构代码</div>
                            </div>
                            @Html.ValidationMessageFor(m => m.工商注册信息.组织机构代码)
                            @if (string.IsNullOrWhiteSpace(Model.工商注册信息.组织机构代码))
                            {
                                <span>*必需</span>
                            }
                        </td>
                        <td align="right">基本账户开户银行：</td>
                        <td>
                            @Html.EditorFor(m => m.工商注册信息.基本账户开户银行)
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">请填写开户银行的名称</div>
                            </div>
                            @Html.ValidationMessageFor(m => m.工商注册信息.基本账户开户银行)
                            @if (string.IsNullOrWhiteSpace(Model.工商注册信息.基本账户开户银行))
                            {
                                <span>*必需</span>
                            }
                        </td>
                    </tr>
                    <tr>
                        <td align="right">基本账户银行帐号：</td>
                        <td>
                            @Html.EditorFor(m => m.工商注册信息.基本账户银行帐号)
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">请填写银行账号,<em style="color:red;">请不要添加空格以及特殊符号！</em></div>
                            </div>
                            @Html.ValidationMessageFor(m => m.工商注册信息.基本账户银行帐号)
                            @if (string.IsNullOrWhiteSpace(Model.工商注册信息.基本账户银行帐号))
                            {
                                <span>*必需</span>
                            }
                        </td>
                        <td width="25%" align="right">
                            @Html.CheckBoxFor(m => m.工商注册信息.基本账户开户银行资信证明, new { onmouseover = "showdiv(this)", onmouseout = "hidediv(this)" })
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">有则勾选，没有则不用勾选</div>
                            </div>
                            @Html.ValidationMessageFor(m => m.工商注册信息.基本账户开户银行资信证明)
                        </td>
                        <td>是否有基本账户开户银行资信证明</td>
                    </tr>
                    <tr>
                        <td align="right">
                            @Html.CheckBoxFor(m => m.工商注册信息.近3年缴纳社会保证金证明, new { onmouseover = "showdiv(this)", onmouseout = "hidediv(this)" })
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">有则勾选，没有则不用勾选</div>
                            </div>
                            @Html.ValidationMessageFor(m => m.工商注册信息.近3年缴纳社会保证金证明)
                        </td>
                        <td>是否有近3年缴纳社会保险金证明</td>
                        <td align="right">
                            @Html.CheckBoxFor(m => m.工商注册信息.近3年有无重大违法记录, new { onmouseover = "showdiv(this)", onmouseout = "hidediv(this)" })
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">有告知函则勾选，没有则不用勾选</div>
                            </div>
                        </td>
                        <td>是否有行贿犯罪档案查询结果告知函</td>
                    </tr>
                    <tr>
                        <td align="right">营业执照发证机关：</td>
                        <td>
                            @Html.EditorFor(m => m.营业执照信息.营业执照发证机关)
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">请填写营业执照发证机关名称</div>
                            </div>
                            @Html.ValidationMessageFor(m => m.营业执照信息.营业执照发证机关)
                            @if (string.IsNullOrWhiteSpace(Model.营业执照信息.营业执照发证机关))
                            {
                                <span>*必需</span>
                            }
                        </td>
                        <td align="right">营业执照注册号：</td>
                        <td>
                            @Html.EditorFor(m => m.营业执照信息.营业执照注册号)
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">请填写营业执照注册号</div>
                            </div>
                            @Html.ValidationMessageFor(m => m.营业执照信息.营业执照注册号)
                            @if (string.IsNullOrWhiteSpace(Model.营业执照信息.营业执照注册号))
                            {
                                <span>*必需</span>
                            }
                        </td>
                    </tr>
                    <tr>
                        <td align="right">营业执照注册地址：</td>
                        <td>
                            @Html.EditorFor(m => m.营业执照信息.营业执照注册地址)
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">请填写营业执照注册地址,如四川省成都市工商局</div>
                            </div>
                            @Html.ValidationMessageFor(m => m.营业执照信息.营业执照注册地址)
                            @if (string.IsNullOrWhiteSpace(Model.营业执照信息.营业执照注册地址))
                            {
                                <span>*必需</span>
                            }
                        </td>
                        <td align="right">营业执照注册资金(万元)：</td>
                        <td>
                            @Html.EditorFor(m => m.营业执照信息.营业执照注册资金)
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">请填写注册资金,如2000</div>
                            </div>
                            @Html.ValidationMessageFor(m => m.营业执照信息.营业执照注册资金)
                            @if (Model.营业执照信息.营业执照注册资金 == 0)
                            {
                                <span>*必需</span>
                            }
                        </td>
                    </tr>
                    <tr>
                        <td align="right">营业执照有效期类型：</td>
                        <td>
                            @Html.EnumDropDownListFor(m => m.营业执照信息.营业执照有效期类型, new { @style = "width:154px; margin-left:2px;display:inline;top:0px; box-shadow:none;-webkit-box-shdow:none", id = "type", onchange = "settype(this)" })
                            <div class="out_box" style="left:159px;">
                                <div class="arrow"></div>
                                <div class="con_box">请选择营业执照的有效期类型</div>
                            </div>
                            @Html.ValidationMessageFor(m => m.营业执照信息.营业执照有效期类型)
                        </td>
                        <td align="right">营业执照有效期起始日期：</td>
                        <td>
                            @Html.TextBoxFor(m => m.营业执照信息.营业执照有效期起始日期, new { onfocus = "WdatePicker({dateFmt:'yyyy/MM/dd'})" })
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">请填写营业执照的有效起始日期</div>
                            </div>
                            @Html.ValidationMessageFor(m => m.营业执照信息.营业执照有效期起始日期)
                        </td>
                    </tr>
                    <tr>
                        <td align="right">营业执照有效期结束日期：</td>
                        <td>
                            @Html.TextBoxFor(m => m.营业执照信息.营业执照有效期结束日期, new { onfocus = "WdatePicker({dateFmt:'yyyy/MM/dd'})", id = "endtime" })
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">请填写营业执照的有效结束日期</div>
                            </div>
                            @Html.ValidationMessageFor(m => m.营业执照信息.营业执照有效期结束日期)
                        </td>
                        <td align="right">营业执照最近年检日期：</td>
                        <td>
                            @Html.TextBoxFor(m => m.营业执照信息.营业执照最近年检日期, new { onfocus = "WdatePicker({dateFmt:'yyyy/MM/dd'})" })
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">请填写营业执照最近的年检日期</div>
                            </div>
                            @Html.ValidationMessageFor(m => m.营业执照信息.营业执照最近年检日期)
                        </td>
                    </tr>
                    <tr>
                        <td valign="top" align="right">经营范围（主营）：</td>
                        <td colspan="3">
                            @if (string.IsNullOrWhiteSpace(Model.营业执照信息.经营范围))
                            {
                                <div id="tip1" style="position:absolute;margin-top:30px; color:#aaa;margin-left:100px; z-index:9;">最多可以添加100个字符</div>
                            }
                            else
                            {
                                 <div id="tip1" style="position:absolute;margin-top:30px; display:none; color:#aaa;margin-left:100px; z-index:9;">最多可以添加100个字符</div>
                            }
                            @Html.TextAreaFor(m => m.营业执照信息.经营范围, new { rows = "5", cols = "90", style = "resize:none;", onfocus = "hideTip(this,'#tip1')", onblur = "showTip(this,'#tip1')", onkeyup = "showOrHide(this,'#tip1')" })
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">请填写企业主营范围</div>
                            </div>
                            @if (string.IsNullOrWhiteSpace(Model.营业执照信息.经营范围))
                            {
                                <span>*必需</span>
                            }
                        </td>
                    </tr>
                    <tr>
                        <td valign="top" align="right">经营范围（兼营）：</td>
                        <td colspan="3">
                            @if (string.IsNullOrWhiteSpace(Model.营业执照信息.兼营范围))
                            {
                                <div id="tip2" style="position:absolute;margin-top:30px; color:#aaa;margin-left:100px; z-index:9;">最多可以添加100个字符</div>
                            }
                            else
                            {
                                <div id="tip2" style="position:absolute;margin-top:30px;display: none; color:#aaa;margin-left:100px; z-index:9;">最多可以添加100个字符</div>
                            }
                            @Html.TextAreaFor(m => m.营业执照信息.兼营范围, new { rows = "5", cols = "90", style = "resize:none;", onfocus = "hideTip(this,'#tip2')", onblur = "showTip(this,'#tip2')", onkeyup = "showOrHide(this,'#tip2')" })
                            <div class="out_box">
                                <div class="arrow"></div>
                                <div class="con_box">请填写企业兼营范围</div>
                            </div>
                            @Html.ValidationMessageFor(m => m.营业执照信息.兼营范围)
                        </td>
                    </tr>
                </table>
            </div>
        </div>
    }
    <div id="box1" style="border:1px solid green; border-bottom:none; margin-top:10px;">
        <form action="/供应商后台/SavePicture" name="upload1" method="post" enctype="multipart/form-data" target="showPic1">
            <input type="hidden" value="orgnize" name="pic1" />
            <table width="100%" style="font-size:14px; margin-left:30px;">
                <tr>
                    <td style="width:120px;" align="right">组织机构代码证电子扫描件：</td>
                    <td style="width:300px;">
                        @if (System.IO.File.Exists(Server.MapPath(@Model.工商注册信息.组织机构代码证电子扫描件)))
                        {
                            <a href="@Model.工商注册信息.组织机构代码证电子扫描件" title="点击查看大图" target="_blank" style="color:red; margin-right:30px; text-decoration:underline;"><img src="@Model.工商注册信息.组织机构代码证电子扫描件" height="30" width="50" /></a>
