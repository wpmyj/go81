        var procount = @Model.中标信息.Count;
        function deletepro(obj) {
            var id = $(obj).attr("id").replace("deleteprobtn___", "");
            $(obj).parent().parent().remove();
            $("#proliststr").val($("#proliststr").val().replace(id + "|", ""));
