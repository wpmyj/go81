                            <td>@Html.DisplayTextFor(m => m.企业基本信息.经营类型)</td>
                            <td align="right"><span style ="font-size:14px;color:black;">经营子类型：</span></td>
                            <td>@Html.DisplayTextFor(m => m.企业基本信息.经营子类型)</td>
                        </tr>
                        <tr>
                            <td align="right"><span style ="font-size:14px;color:black;">注册号：</span></td>
                            <td>@Html.DisplayTextFor(m => m.营业执照信息.营业执照注册号)</td>
                            <td align="right"><span style ="font-size:14px;color:black;">法定代表人：</span></td>
                            <td>@Html.DisplayTextFor(m => m.法定代表人信息.法定代表人姓名)</td>
                        </tr>
                        <tr>
                            <td align="right"><span style ="font-size:14px;color:black;">员工人数：</span></td>
                            <td>@Html.DisplayTextFor(m => m.企业基本信息.员工人数)</td>
                            <td align="right"><span style ="font-size:14px;color:black;">经济性质：</span></td>
                            <td>@Html.DisplayTextFor(m => m.企业基本信息.经济性质)</td>
                        </tr>
                        <tr>
                            <td align="right"><span style ="font-size:14px;color:black;">企业网址：</span></td>
                            <td><a href="@Model.企业基本信息.网站网址">@Model.企业基本信息.网站网址</a></td>
                            <td align="right"><span style ="font-size:14px;color:black;">邮政编码：</span></td>
                            <td>@Html.DisplayTextFor(m => m.企业基本信息.邮政编码)</td>
                        </tr>
                        <tr>
                            <td align="right"><span style ="font-size:14px;color:black;">是否是协议供货商：</span></td>
                            @if (Model.供应商用户信息.协议供应商)
                            {
                                <td><div style="width:30px; height:30px; background-image:url(/Images/qualifies.png);background-position-x:60px;background-position-y:0px;"></div></td>
                            }
                            else
                            {
                                <td>否</td>
                            }
                            <td align="right"><span style ="font-size:14px;color:black;">是否是应急供应商：</span></td>
                            @if (Model.供应商用户信息.应急供应商)
                            {
                                <td><div style="width:30px; height:30px; background-image:url(/Images/qualifies.png);background-position-x:90px;background-position-y:0px;"></div></td>
                            }
                            else
                            {

                                <td>否</td>
                            }
                        </tr>
                        <tr>
                            <td align="right"><span style ="font-size:14px;color:black;">是否年检：</span></td>
                            <td colspan="3">
                                @if (Model.供应商用户信息.年检列表 != null && Model.供应商用户信息.年检列表.Any() && Model.供应商用户信息.年检列表.ContainsKey(DateTime.Now.Year.ToString()))
                                {
                                    <div style="width:30px; height:30px; background-image:url(/Images/qualifies.png);background-position-x:120px;background-position-y:0px;"></div>
                                }
                                else
                                {
                                    <span style="color:red;">未通过年检</span>
                                }
                            </td>
                        </tr>
                        <tr><td align="right"><span style ="font-size:14px;color:black;">所在区域：</span></td><td colspan="3">@Model.所属地域.地域</td></tr>
                        @if (Model.可提供产品类别列表 != null && Model.可提供产品类别列表.Count() != 0)
                        {
                            <tr><td colspan="4" align="center"><h2>可提供产品类别</h2></td></tr>
                            <tr>
                                <td align="center"><span style="font-weight:bolder;">一级产品名称</span></td>
                                <td align="center" colspan="3"><span style="font-weight:bolder;">二级产品名称</span></td>
                            </tr>
                            for (int i = 0; i < Model.可提供产品类别列表.Count(); i++)
                            {
                                <tr>
                                    <td align="center">@Model.可提供产品类别列表[i].一级分类</td>
                                    <td colspan="3">
                                        @if (Model.可提供产品类别列表[i].二级分类 != null && Model.可提供产品类别列表[i].二级分类.Count() != 0)
                                        {
                                            foreach (var item in Model.可提供产品类别列表[i].二级分类)
                                            {
                                                <span>@item,</span>
                                            }
                                        }
                                    </td>
                                </tr>
                            }
                        }
                        <tr><td align="center" colspan="4"><h2>联系人信息</h2></td></tr>
                        <tr>
                            <td width="20%" align="right"><span style ="font-size:14px;color:black;">联系人姓名：</span></td>
                            <td width="30%">@Model.企业联系人信息.联系人姓名</td>
                            <td width="20%" align="right"><span style ="font-size:14px;color:black;">联系人性别：</span></td>
                            <td width="30%">@Model.企业联系人信息.联系人性别</td>
                        </tr>
                        <tr>
                            <td align="right"><span style ="font-size:14px;color:black;">联系人手机：</span></td>
                            <td>@Model.企业联系人信息.联系人手机</td>
                            <td align="right"><span style ="font-size:14px;color:black;">联系人固定电话：</span></td>
                            <td>@Model.企业联系人信息.联系人固定电话</td>
                        </tr>
                        <tr>
                            <td align="right"><span style ="font-size:14px;color:black;">联系人传真：</span></td>
                            <td>@Model.企业联系人信息.联系人传真</td>
                            <td align="right"><span style ="font-size:14px;color:black;">联系人地址：</span></td>
                            <td>@Model.企业联系人信息.联系人地址</td>
                        </tr>
                        @if (Model.售后服务机构列表 != null && Model.售后服务机构列表.Count() != 0)
                        {
                            int j = 1;
                            <tr><td colspan="4" align="center"><h2>售后服务机构表</h2></td></tr>
                            foreach (var item in Model.售后服务机构列表)
                            {
                                <tr>
                                    <td align="right"><span style ="font-size:14px;color:black;">@(j++)、服务机构名称：</span></td>
                                    <td>@item.售后服务机构名称</td>
                                    <td align="right" style="font-size:14px;color:black;"><span>服务机构类别：</span></td>
                                    <td>@item.售后服务机构类别.ToString()</td>
                                </tr>
                                <tr>
                                    <td align="right"><span style ="font-size:14px;color:black;">服务机构所在地：</span></td>
                                    <td>@(item.所在地.省份)-@(item.所在地.城市)-@(item.所在地.地域)</td>
                                    <td align="right"><span style ="font-size:14px;color:black;">服务机构联系人：</span></td>
                                    <td>@item.负责人联系方式.联系人</td>
                                </tr>
                                <tr>
                                    <td align="right"><span style ="font-size:14px;color:black;">联系手机号：</span></td>
                                    <td colspan="3">@item.负责人联系方式.手机</td>
                                </tr>
                            }
                        }
                    </table>
                </div>
