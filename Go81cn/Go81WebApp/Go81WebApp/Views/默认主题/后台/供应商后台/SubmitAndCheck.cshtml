<div class="bg_outdiv">
    <div class="bg_outdiv_left">
        @Html.Action("LeftMenu", "布局")
    </div>
    <div class="bg_outdiv_right">
        <div class="gys_znxx_topbox">
            <div class="gys_xttz_topbg">
                <div class="index_bottom_zb_top_title">企业详情</div>
            </div>
        </div>
        <div class="gys_znxx_box">
            <div>
                <div style="margin-top:20px;width:100%;" class="Tab">
                    <a onmouseover="checks(this,'tb2')">企业概况</a><a onmouseover="checks(this,'tb3')">企业基本信息</a><a onmouseover="checks(this,'tb4')">工商注册信息</a><a onmouseover="checks(this,'tb5')">营业执照信息</a><a onmouseover="checks(this,'tb6')">法定代表人信息</a><a onmouseover="checks(this,'tb7')">出资人或股东</a><a onmouseover="checks(this,'tb8')">财务状况信息</a><a onmouseover="checks(this,'tb9')" style="border-right:none;">税务信息</a>
                </div>
                <div class="tb2">
                    <table cellspacing="1" width="100%">
                        <tr>
                            <td colspan="4"><span style ="font-size:14px;color:black;">企业简介：</span>@Html.DisplayTextFor(m => m.企业基本信息.企业简介)</td>
                        </tr>
                    </table>
                </div>
                <div class="tb3">
                    <table cellspacing="1" width="100%">
                        <tr>
                            <td align="right"><span style ="font-size:14px;color:black;">企业名称：</span></td>
                            <td>@Html.DisplayTextFor(m => m.企业基本信息.企业名称)</td>
                            <td align="right"><span style ="font-size:14px;color:black;">注册地址：</span></td>
                            <td>@Html.DisplayTextFor(m => m.企业基本信息.注册地址)</td>
                        </tr>
                        <tr>
                            <td align="right"><span style ="font-size:14px;color:black;">成立时间：</span></td>
                            <td>@Html.DisplayTextFor(m => m.企业基本信息.成立时间)</td>
