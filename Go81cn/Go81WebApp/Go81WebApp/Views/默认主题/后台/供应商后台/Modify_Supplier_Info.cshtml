                                        @if (System.IO.File.Exists(Server.MapPath(@Model.营业执照信息.三证合一扫描件)))
                                        {
                                            <input type="checkbox" value="together" onclick="checkThis(this)" id="together" checked="checked" /><span style="font-size:18px;">三证合一</span>
                                        }
                                        else
                                        {
                                            <input type="checkbox" value="together" onclick="checkThis(this)" id="together" /><span style="font-size:18px;">三证合一</span>
                                        }
                                        <form action="/供应商后台/SavePicture" name="upload0" method="post" enctype="multipart/form-data" target="showPic0">
                                            @if (System.IO.File.Exists(Server.MapPath(@Model.营业执照信息.三证合一扫描件)))
                                            {
                                                <div id="allpic" style="margin-right:10px;">
                                                    <div id="box0" style="border:1px solid green; margin-top:10px; border-bottom:none;">

                                                        <input type="hidden" value="together" name="pic1" />
