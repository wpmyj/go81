                                                            <tr>
                                                                <td style="width:120px;" align="right">三证合一扫描件：</td>
                                                                <td style="width:300px;">
                                                                    @if (System.IO.File.Exists(Server.MapPath(@Model.营业执照信息.三证合一扫描件)))
                                                                    {
