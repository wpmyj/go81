                                                <select id="delivercity" name="delivercity" style="width:171px; margin-left:2px;" class="info"></select>
                                                <select id="deliverarea" name="deliverarea" style="width:171px; margin-left:2px;" class="info"></select><span class="marker">*</span>
                                                <script>
                                                    window.onload = function () { new PCAS("deliverprovince", "delivercity", "deliverarea", "@Model.所属地域.省份", "@Model.所属地域.城市", "@Model.所属地域.区县"); }

                                                </script>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td align="right" valign="top">展示图片：</td>
                                            <td colspan="3">
                                                @if (Model.供应商用户信息.供应商图片 != null && Model.供应商用户信息.供应商图片.Count() != 0)
                                                {
                                                    for (int i = 0; i < Model.供应商用户信息.供应商图片.Count(); i++)
                                                    {
                                                        if (System.IO.File.Exists(Server.MapPath(@Model.供应商用户信息.供应商图片[i])))
                                                        {
                                                            <span style="display:inline-block;">
                                                                <a style="display:block; position:absolute; width:15px;height:15px; margin-left:85px; background-color:red; cursor:pointer; line-height:15px; text-align:center; color:white;" title="删除" lang="@Model.供应商用户信息.供应商图片[i]" onclick="delPic(this)">×</a>
                                                                <a target="_blank" title="点击查看大图" href="@(Model.供应商用户信息.供应商图片[i])"><img height="50" width="100" src="@(Model.供应商用户信息.供应商图片[i])" /></a>
                                                            </span>
                                                        }
                                                    }


                                                }
                                                <span style="display:none;">
                                                    <input type="file" style="display:inline; width:120px;" name="file" />
                                                    <em style="color:red;">注：请上传2M以下的jpg格式的彩色图片</em>
                                                </span>
                                                <a href="###" onclick="modify(this)" style="color:red; font-size:16px; text-decoration:underline;">添加展示图</a>
                                                <span></span>
                                            </td>

                                        </tr>
                                        <tr>
                                            <td align="right" class="lab" valign="top">企业简介：</td>
                                            <td align="left" colspan="3">

                                                @Html.TextAreaFor(m => m.企业基本信息.企业简介, new { cols = "80", rows = "5", style = "resize:none;", @class = "info" })<span class="marker">*</span>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div style="margin-left:0px;border-color:rgb(207, 197, 197); margin-top:0px;margin-bottom: 10px; border-style:solid; border-width:1px;">
                            <div id="title_div" class="index_bottom_zb_top_title title_addsupplier">联系人信息</div>
                            <div class="gys_znxx_box" style="width:95%; margin-left:auto; margin-right:auto; margin-bottom:10px;">
                                <table width="80%" style="margin-bottom:10px;" cellspacing="5">
                                    <tr>
                                    <td width="20%" align="right">
                                        联系人：
                                    </td>
                                    <td width="25%">

                                        @Html.TextBoxFor(m => m.企业联系人信息.联系人姓名, new { @class = "info" })<span class="marker">*</span>
                                    </td>
                                    <td width="25%" align="right">
                                        性别：
                                    </td>
                                    <td width="25%">
                                        @Html.EnumDropDownListFor(m => m.企业联系人信息.联系人性别, new { @style = "width:171px; margin-left:2px;", @class = "info" })<span class="marker">*</span>
                                    </td>
                                    <tr>
                                    <td align="right">

                                        电子邮件：
                                    </td>
                                    <td>

                                        @Html.TextBoxFor(m => m.企业联系人信息.联系人邮箱, new { @class = "info" })<span class="marker">*</span>
                                    </td>
                                    <td align="right">
                                        手机：
                                    </td>
                                    <td>
                                        @Html.TextBoxFor(m => m.企业联系人信息.联系人手机, new { @class = "info" })<span class="marker">*</span>

                                    </td>
                                    <tr>
                                        <td align="right">
                                            固定电话：
                                        </td>
                                        <td>

                                            @Html.TextBoxFor(m => m.企业联系人信息.联系人固定电话, new { @class = "info" })<span class="marker">*</span>

                                        </td>
                                        <td align="right">
                                            微信：
                                        </td>
                                        <td>
                                            @Html.TextBoxFor(m => m.企业联系人信息.联系人传真, new { @class = "info" })<span class="marker">*</span>
                                        </td>
                                    </tr>
                                    <tr><td align="right">联系人地址：</td><td colspan="3">@Html.TextBoxFor(m => m.企业联系人信息.联系人地址, new { @class = "info" })<span class="marker">*</span></td></tr>
                                </table>
                            </div>

                        </div>
                        <div style="margin-left:0px;border-color:rgb(207, 197, 197); margin-top:0px;margin-bottom: 10px; border-style:solid; border-width:1px;">
                            <div id="title_div" class="index_bottom_zb_top_title title_addsupplier">营业信息</div>
