                                                    @for (int i = 0; i < Model.历史参标记录.Count; i++)
                                                    {
                                                        <tr>
                                                            <td width="30%">@Model.历史参标记录[i].项目名称</td>
                                                            <td width="20%">@Model.历史参标记录[i].招标方</td>
                                                            <td width="10%">@Model.历史参标记录[i].参标结果</td>
                                                            <td width="10%">@Model.历史参标记录[i].参标时间.ToString("yyyy-MM-dd")</td>
                                                            <td width="10%">
                                                                @if (Model.历史参标记录[i].本平台项目)
                                                                {
                                                                    <span>是</span>
                                                                }
                                                                else
                                                                {
                                                                    <span>否</span>
                                                                }
                                                            </td>
                                                            <td width="10%"><a href="/供应商后台/Modify_Toubiao?id=@Model.Id&index=@i">修改</a><a href="/供应商后台/Delete_Toubiao?index=@i">删除</a></td>
                                                        </tr>
                                                    }
                                                </table>
                                            }
                                            else
                                            {
                                                <h1 style="width: 100%; text-align: center; color: red;"><em>没有招投标信息，点击【新增】按钮添加招投标信息</em></h1>
                                            }
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                if (Model.审核数据.审核状态 != 审核状态.审核通过)
                {
                    if (!Model.供应商用户信息.提交入网)
                    {
                        <div style="text-align:center;">
                            <input class="front_login_rightbox_button" style="width:100px;" type="button" onclick="subform()" value="入网提交" />
                        </div>
                    }
                }
            }
        </div>
    </div>
</div>

<script type="text/javascript" language="javascript">
    function fileChange(target, str1, str2) {
        $(str1).children("#framebox").show();
        document.forms[str2].submit();
        $("#f1").show();
        $(target).prev("span").hide();
    }
    function checkThis(th) {
        if (th.checked == true) {
            $("#allpic").show();
            $("#depart").hide();
        }
        else {
            $("#allpic").hide();
            $("#depart").show();
        }
    }
    function showButton(th, str) {
        $(":file[name='" + str + "']").show();
        $(th).hide();
    }
    $(document).ready(function (e) {
        $(".list a:first").css({ "background-color": "rgb(94, 111, 0)", "color": "white" });

        $("#入网信息管理").addClass("left_menu_select").children("a").css({ "color": "#fff" });
        $("#入网信息管理").parents().attr("show", "true").show().prev("li").addClass("open").children("span").css({ "background": "url('../Images/tree_icons.png') -115px -4px" });
    });
    function checks(th, str) {
        $(".list a").each(function () {
            $(this).css({ "background-image": "url(/images/index_content_bg.jpg)", "color": "black" });
        });
        $(th).css({ "background-color": "rgb(94, 111, 0)", "color": "white", "background-image": "" });
    }
    function subform() {
        var isComplete = true;
        $("div#containerbox :input.info").each(function () {
            var str = $(this).val();
            if (str == "" || str == "0") {
                $(this).css("border", "1px solid #ff0000");
                isComplete = false;
            } else {
                $(this).css("border", "1px solid #BFBEBE");
            }
        });
        if (!isComplete) {
            alert("您还没有完善所有入网的信息，请继续完善");
            return;
        }
        document.forms["data"].submit();
    }
    $(function () {
        var optionlist = $(".industry").children();
        optionlist.each(function (index, domEle) {
            if ($(domEle).text() == $("#hidden").val()) {
                $(domEle).attr("selected", "selected");
            }
        });
    });
    $(".industry").change(function () {
        var val = $(".industry option:selected").text();
        $("#hidden").val(val);
    });
    $("#deliverprovince").change(function () {
        var val = $("#deliverprovince option:selected").text();
        $("#provence").val(val);
    });
    $("#deliverprovince").change(function () {
        var val = $("#deliverprovince option:selected").text();
        $("#provence").val(val);
    });
    $("#delivercity").change(function () {
        var val = $("#delivercity option:selected").text();
        $("#city").val(val);
    });
    $("#deliverarea").change(function () {
        var val = $("#deliverarea option:selected").text();
        $("#area").val(val);
    });
    $("#submit_btn").click(function () {
        var val = $("#delivercity").find("option:selected").text();
        var vall = $("#deliverarea").find("option:selected").text();
        $("#city").val(val);
        $("#area").val(vall);
    });
    $("document").ready(function () {
        $(":checkbox.nianshen").click(function () {
            if (this.checked) {
                $(this).val("1");
            } else {
                $(this).val("0");
            }
        });
        $("select.twoCls").blur(function () {
            var val = $(this).val();
            $("#twoClass").val(val);
        });
    });
    $("#area1").click(function () {
        if (this.checked) {
            $("#gysarea").show(500);
        }
    });
    $("a").each(function () {
        if ($(this).text() == "修改" || $(this).text() == "删除" || $(this).text() == "查看") {
            $(this).css("color", "red");
            $(this).mouseover(function () { $(this).css("text-decoration", "underline"); });
            $(this).mouseout(function () { $(this).css("text-decoration", "none"); });
        }
    });
    function message(th) {
        if (th.checked) {
            $(th).val(1);
        }
        else {
            $(th).val(-1);
        }
    }
    $(document).ready(function () {
        var val = $("#IsSubmit").val();
        if (val == "1") {
            $("div#containerbox :input").attr("disabled", true);
        }
    });
    function show_tip(th) {
        $(th).next("div").show();
        $(th).next("div").css("display", "inline-block");
    }
    function hide_tip(th) {
        $(th).next("div").hide();
    }
    function showBox(th) {
        $(th).show(500);
    }
    function hidebox(th) {
        $(th).hide(500);
    }
    function Reason(th) {
        if ($(th).val() == "2") {
            $("#f").show();
        }
    }
    function setReason(obj) {
        var r = $(obj).val();
        $("#reason").val(r);
        $("#f").hide();
    }
    function modify(th) {
        $(th).prev("span").show();
        $(th).next("span").remove();
        $(th).remove();
    }
    function delPic(th) {
        if (confirm("确定删除吗？")) {
            var path = $(th).attr("lang");
            $.get('/供应商后台/Del_gysPic', { n: path },
                function (data) {
                    if (data == 1) {
                        $(th).parent().remove();
                    }
                });
        }
    }
</script>
