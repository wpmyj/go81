@model Go81WebApp.Models.数据模型.用户数据模型.供应商
@using Go81WebApp.Models.数据模型
@using Go81WebApp.Models.数据模型.商品数据模型
@{
    ViewBag.Title = "修改供应商信息";
}
<link href="~/css/css.css" rel=" stylesheet" />
<link href="~/css/index.css" rel="stylesheet" />
<link href="~/css/Msg_Tips.css" rel="stylesheet" />
<script src="~/JS/provinceandcity_all.js"></script>
<script type="text/javascript" src="/JS/My97DatePicker/calendar.js"></script>
<script type="text/javascript" src="/JS/My97DatePicker/config.js"></script>
<script type="text/javascript" src="/JS/My97DatePicker/WdatePicker.js"></script>
<style>
    table tr td span {
        font-size: 12px;
        color: red;
    }

    table tr td {
        font-size: 14px;
    }

    select {
        position: relative;
    }

    .good_link {
        color: red;
    }

    .good_link:hover {
        text-decoration: underline;
        color: red;
    }

    .list {
        font-size: 14px;
        width: 1003px;
        background-image: url(/images/index_content_bg.jpg);
    }

    .list a {
        width: 100px;
        cursor: pointer;
        height: 30px;
        color: #336;
        display: inline-block;
        text-align: center;
        line-height: 30px;
    }

    .submenulist li h3 a.on {
        width: 241px;
        color: #006400;
        border: 1px solid #aaa;
        border-right: none;
        border-left: none;
        overflow: hidden;
        margin: 0;
        height: 30px;
        font-size: 14px;
        line-height: 30px;
        background: #fff;
        box-shadow: -10px 0 14px #ccc;
        z-index: 9999;
        text-decoration: none;
    }
    .marker {
        color: #ff0000;
    }
</style>
<div id="f1" class="modal_out">
    <div class="modal_in"></div>
    <div class="modal_content_loading">
        <img src="~/Images/ajax-loader.gif" />
        <span>正在上传，请稍等...</span>
    </div>
</div>
<div id="Toubiao" style="display:none;">
    <div style="position:fixed; z-index:20; top:0px; left:0px; opacity:0.5;-moz-opacity:0.5;filter:alpha(opacity=50); text-align:center; right:0; bottom:0px; background-color:black; height:100%; width:100%;border:1px solid green; box-shadow:0px 0px 10px gray; -webkit-box-shadow:0px 0px 10px gray;-moz-box-shadow:0px 0px 10px gray;">
    </div>
    <div style="width:100%;position:fixed; z-index:21; left:0px;top:20%;min-height:330px; font-size:18px; text-align:left; padding-left:20px; line-height:30px;">
        <div style="width:600px;position:relative; border:1px solid red; margin:0px auto; height:300px; font-size:18px; text-align:left; padding-left:20px; line-height:30px; background-color:white;">
            <div style="width:100%; height:auto;"><a style="width:30px; height:30px; line-height:30px; text-align:center; margin-top:5px; margin-right:5px; background-color:red; font-size:30px; color:white; float:right;cursor:pointer;" title="关闭窗口" onclick="hidebox('#Toubiao')">×</a></div>
            <div style="float:right; width:100%; font-size:12px;">
                <h2 style="text-align:center;">新增招投标经历</h2>
                <form action="/供应商后台/Toubiao_Manage" method="post">
                    @Html.HiddenFor(m => m.Id)
                    <table width="90%">
                        <tr>
                            <td width="20%" height="30" align="right">项目名称：</td>
                            <td width="30%" height="30"><input onmouseover="show_tip(this)" onmouseout="hide_tip(this)" type="text" name="历史参标记录[0].项目名称" /></td>

                            <td width="20%" height="30" align="right">招标方：</td>
                            <td width="30%" height="30"><input onmouseover="show_tip(this)" onmouseout="hide_tip(this)" type="text" name="历史参标记录[0].招标方" /></td>

                        </tr>
                        <tr>
                            <td align="right" height="30">参标结果：</td>
                            <td style="position:relative;"><select onmouseover="show_tip(this)" onmouseout="hide_tip(this)" name="历史参标记录[0].参标结果"><option value="0">请选择参标结果</option><option value="1">中标</option><option value="2">落标</option><option value="3">流标</option><option value="4">废标</option><option value="9999">其他</option></select><div class="out_box" style="left:155px;"><div class="arrow"></div><div class="con_box">请选择参标结果</div></div></td>

                            <td align="right" height="30">参标时间：</td>
                            <td><input onmouseout="hide_tip(this)" onmouseover="show_tip(this)" onfocus="WdatePicker({ isShowWeek: true })" type="text" name="历史参标记录[0].参标时间" /></td>
                        </tr>
                        <tr><td align="right" style="position:relative;"><input onmouseover="show_tip(this)" onmouseout="hide_tip(this)" value="true" type="checkbox" name="历史参标记录[0].本平台项目" /><div class="out_box" style="left:155px;"><div class="arrow"></div><div class="con_box" style="min-width:120px;">是本平台项目，请选择，否则，不用选</div></div></td><td>是否是本平台项目</td></tr>
                    </table>
                    <table width="100%" style="margin-top:20px;"><tr><td style="display:block;height:50px; text-align:center;"><input type="submit" id="submit_btn" value="保  存" /></td></tr></table>
                </form>
            </div>
        </div>
    </div>
</div>
<div class="bg_outdiv">
    <div class="bg_outdiv_left">
        @Html.Action("LeftMenu", "布局")
    </div>
    <div class="bg_outdiv_right" style="z-index:1;">
        <div class="gys_znxx_topbox">
            <div class="gys_xttz_topbg">
                <div class="index_bottom_zb_top_title">
                    企业入网信息管理
                                @if (Model.供应商用户信息.提交入网)
                                {
                                    <span style="color: #ff0000;">（您已成功提交入网，如需修改信息，请联系网站客服）</span>
                                }
                </div>
            </div>
        </div>
        <div id="containerbox">
            @if (!Model.供应商用户信息.提交入网)
            {
                <div style="border: 1px solid #ff0000;margin-top: 10px;padding:5px;">
                    <span style="color: #ff0000;">
                        请您完善企业入网信息,并提交审核。
                        供应商完善入网信息后,将成为西南物资采购网的入网供应商,部队单位用户即可在网站上查询了解您的企业和产品信息。
                    </span>
                </div>
            }
            @using (Html.BeginForm("Modify_SupplierInfo", "供应商后台", FormMethod.Post, new { enctype = "multipart/form-data", name = "data" }))
            {
                if (Model.审核数据.审核状态 != 审核状态.审核通过)
                {
                    if (Model.供应商用户信息.提交入网)
                    {
                        <input type="hidden" value="1" name="IsSubmit" id="IsSubmit" />
                    }
                    else
                    {
                        <input type="hidden" value="2" name="IsSubmit" id="IsSubmit" />
                    }
                }
                else
                {
                    <input type="hidden" value="1" name="IsSubmit" id="IsSubmit" />
                }
                @Html.HiddenFor(m => m.Id)
                <div class="gys_znxx_box" style="border:none;">
