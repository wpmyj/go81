                                    <table width="90%" style="margin-left:30px;">
                                        <tr>
                                            <td width="15%" align="right">企业中文名称：</td>
                                            <td width="35%">
                                                @Html.TextBoxFor(m => m.企业基本信息.企业名称,new{@class="info"})<span class="marker">*</span>
                                            </td>
                                            <td width="25%" align="right" class="lab">企业英文名称：</td>
                                            <td width="25%">
                                            @Html.TextBoxFor(m => m.企业基本信息.英文名称)
                                        </tr>
                                        <tr>
                                            <td width="25%" align="right" class="lab">企业简称：</td>
                                            <td width="25%">
                                                @Html.TextBoxFor(m => m.企业基本信息.简称)
                                            </td>
                                            <td width="15%" align="right">注册地址：</td>
                                            <td width="35%">
                                                @Html.TextBoxFor(m => m.企业基本信息.注册地址, new { @class = "info" })<span class="marker">*</span>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td align="right" class="lab">邮政编码：</td>
                                            <td>
                                                @Html.TextBoxFor(m => m.企业基本信息.邮政编码, new { @class = "info" })<span class="marker">*</span>
                                            </td>
                                            <td align="right">成立时间：</td>
                                            <td>
                                                @Html.TextBoxFor(m => m.企业基本信息.成立时间, new { onfocus = "WdatePicker({isShowWeek:true})", @class = "info" })<span class="marker">*</span>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td align="right" class="lab">网站地址：</td>
                                            <td>
                                                @Html.TextBoxFor(m => m.企业基本信息.网站网址)
                                            </td>
                                        </tr>
                                        <tr>
                                            <td align="right" class="lab">经营类型：</td>
                                            <td>

                                                @Html.EnumDropDownListFor(m => m.企业基本信息.经营类型, new { @style = "width:171px; margin-left:2px;", @class = "info" })<span class="marker">*</span>
                                            </td>
                                            <td align="right" class="lab">经营子类型：</td>
                                            <td>
                                                @Html.EnumDropDownListFor(m => m.企业基本信息.经营子类型, new { @style = "width:171px; margin-left:2px;", @class = "info" })<span class="marker">*</span>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td align="right" class="lab">员工人数：</td>
                                            <td style="position: relative;">

                                                @Html.EnumDropDownListFor(m => m.企业基本信息.员工人数, new { @style = "width:171px; margin-left:2px;", @class = "info" })<span class="marker">*</span>
                                            </td>
                                            <td align="right" class="lab">经济性质：</td>
                                            <td>
                                                @Html.EnumDropDownListFor(m => m.企业基本信息.经济性质, new { @style = "width:171px; margin-left:2px;", @class = "info" })<span class="marker">*</span>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td align="right" class="lab">所属地域：</td>
                                            <td colspan="3">
                                                @Html.HiddenFor(m => m.所属地域.省份, new { id = "provence",@class="info"})
                                                @Html.HiddenFor(m => m.所属地域.城市, new { id = "city",@class="info" })
                                                @Html.HiddenFor(m => m.所属地域.区县, new { id = "area",@class="info" })
