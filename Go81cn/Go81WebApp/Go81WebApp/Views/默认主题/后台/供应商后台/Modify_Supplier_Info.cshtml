                                <div style="margin-left:20px;">
                                    <table width="100%" style="margin-left:-30px; margin-top:10px; margin-bottom:10px;">
                                        <tr>
                                            <td width="25%" align="right">营业执照注册号：</td>
                                            <td width="25%">
                                                @Html.TextBoxFor(m => m.营业执照信息.营业执照注册号, new { @class = "info" })<span class="marker">*</span>
                                            </td>
                                            <td width="25%" align="right">营业执照注册资金：</td>
                                            <td width="25%">
                                                @Html.TextBoxFor(m => m.营业执照信息.营业执照注册资金, new { @class = "info" })万元<span class="marker">*</span>
                                            </td>
                                        </tr>
                                    </table>
