@{
    Layout = null;
}
<html lang="zh-cn">
<head>
    <title>功能选择首页</title>
    <script src="~/JS/jquery-1.6.min.js"></script>
    <script src="~/JS/applayapp.js" type="text/javascript"></script> 
        <style>
        #boxContainer {
            width:610px;
            height: 150px;
            text-align: center;
            margin: 0px auto;
        }
        #header {
            text-align: center;
            font-size:35px;
            line-height: 60px;
            font-weight: bolder;
            margin-top: 100px;
            margin-bottom: 50px;
        }
        .directUlr {
            text-decoration: none;
            display: block;
            float: left;
            width:300px;
            height: 150px;
            line-height: 150px;
            font-size:38px;
            font-weight: bold;
            color: #fff;
            text-align: center;
            background: -moz-linear-gradient(top,#4169e1,#000080); /*火狐*/
            background: -webkit-gradient(linear, 0% 0%, 0% 100%,from(#4169e1), to(#000080)); /*谷歌*/
            background-image: -webkit-gradient(linear,left bottom,left top,color-start(0, #4169e1),color-stop(1,#000080)); /* Safari & Chrome*/
            filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#4169e1', endColorstr='#000080'); /*IE6 & IE7*/
            -ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#4169e1', endColorstr='#000080')"; /* IE8 */
            FILTER: progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#4169e1,endColorStr=#000080); /*IE*/
            }

            .directUlr:hover {
                background: -moz-linear-gradient(top,#000080,#4169e1); /*火狐*/
                background: -webkit-gradient(linear, 0% 0%, 0% 100%,from(#000080), to(#4169e1)); /*谷歌*/
                background-image: -webkit-gradient(linear,left bottom,left top,color-start(0, #000080),color-stop(1,#4169e1)); /* Safari & Chrome*/
                filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#000080', endColorstr='#4169e1'); /*IE6 & IE7*/
                -ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#000080', endColorstr='#4169e1')"; /* IE8 */
                FILTER: progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#000080,endColorStr=#4169e1); /*IE*/
                cursor: pointer;
            }
    </style>
</head>
    <body style="text-align:center;">
        <div id="header">欢迎使用供应商和评审专家抽取系统</div>
        <div id="boxContainer">
            <a class="directUlr btn_target" href="/专家抽选/Expert_Applay_Sapp">评审专家抽选</a>
            <a class="directUlr btn_target" href="/专家抽选/GysChoose_Applay_SApp" style="margin-left:10px;">供应商抽选</a>
        </div>
    </body>
</html>

