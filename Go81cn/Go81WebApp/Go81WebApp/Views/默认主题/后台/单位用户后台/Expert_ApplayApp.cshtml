                        <script type="text/javascript">
                        </script>
                    </div>

                </div>
            </div>
        </div>
        <div id="modaltips" class="modal fade" style="display:block;position:relative; height:100%; *+top:expression(eval(document.compatMode && document.compatMode=='CSS1Compat') ? documentElement.scrollTop + (documentElement.clientHeight/25)-20 : document.body.scrollTop + (document.body.clientHeight/25)-20);">
            <div class="modal-dialog" style="margin: 2% auto;">
                <div class="modal-content" style="height:320px; width: 840px;">
                    <div class="modal-header" style="padding-bottom: 20px;">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true" onclick='$("#modaltips").hide();'>×</button>
