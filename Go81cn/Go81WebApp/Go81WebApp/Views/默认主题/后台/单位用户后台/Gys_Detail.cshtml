<script type="text/javascript">
    function changeauditgysmail(th) {
        if (th.checked) {
            $(th).val("1");
        }
        else {
            $(th).val("0");
        }
    }
    function checks(th,str) {
        $(".Tab a").each(function () {
            $(this).css({ "background": "none" });
        });
        $(th).css({"background": "url(/images/tabbg.jpg)"});
        $(".tb2,.tb3,.tb4,.tb5,.tb6,.tb7,.tb8,.tb9").hide();
        $("."+str).show();
    }

    function SubmitData() {
        $("form[name='data']").attr("action", "/单位用户后台/Supplier_Refused");
        $("form[name='data']").submit();
    }
    $("document").ready(function () {
        $(".Tab a:first").css({"background":"url(/images/tabbg.jpg)"});
        $(".tb2").show();
        $(":checkbox#ns").click(function () {
            if (this.checked) {
                $(this).val("1");
            }
            else { $(this).val("0"); }
        });
    });
    var areaCount = 0;
    function addarea() {
        if ($('#deliverprovince1').val() == ""||$('#delivercity1').val() == ""||$('#deliverarea1').val() == "") {
            alert("请将省份，城市，区县填写完整");
        }
        else {
            var have = false;
            for (var i = 0; i < $("#supplierarea :input").length; i++) {
                if ($("#supplierarea :input:eq(" + i + ")").val() == $('#deliverprovince1').val() + $('#delivercity1').val() + $('#deliverarea1').val()) {
                    alert("您已经添加过了！");
                    have = true;
                    break;
                }
            }
            if (have == false) {
                $("#supplierarea").append("<input type='hidden' name='供应商用户信息.协议供应商所属地区[" + areaCount + "].省份' value='" + $('#deliverprovince1').val() + "'/><input type='hidden' name='供应商用户信息.协议供应商所属地区[" + areaCount + "].城市' value='" + $('#delivercity1').val() + "'/><input type='hidden' name='供应商用户信息.协议供应商所属地区[" + areaCount + "].区县' value='" + $('#deliverarea1').val() + "'/><input type='hidden' name='" + areaCount + "' value='" + $('#deliverprovince1').val() + $('#delivercity1').val() + $('#deliverarea1').val() + "'/>");
                $("#newarea").append("<tr><td>" + $('#deliverprovince1').val() + "</td><td>" + $('#delivercity1').val() + "</td><td>" + $('#deliverarea1').val() + "</td><td>" + $('#deliverprovince1').val() + $('#delivercity1').val() + $('#deliverarea1').val() + "</td></tr>");
                areaCount++;
            }

        }
    }
    $("#area1").click(function () {
        if (this.checked) {
            $("#gysarea").show(500);
        }
    });
    function hidebox1(th) {
        $(th).hide(500);
    }
    function hidebox() {
        $("div#f").hide(500);
    }
    function showbox() {
        $("div#f").show(500);
    }
   
    var sd = @ViewData["已审核"];
    
    if(sd == "0")
    {
        $(function () {
            $("#待审核供应商").addClass("left_menu_select").children("a").css({ "color": "#fff" });
            $("#待审核供应商").parents().attr("show", "true").show().prev("li").addClass("open").children("span").css({ "background": "url('../Images/tree_icons.png') -115px -4px" });
        });
    }
    else
    {
        $(function () {
            $("#已审核供应商").addClass("left_menu_select").children("a").css({ "color": "#fff" });
            $("#已审核供应商").parents().attr("show", "true").show().prev("li").addClass("open").children("span").css({ "background": "url('../Images/tree_icons.png') -115px -4px" });
        });
    }
    
</script>


