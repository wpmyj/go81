                <td class="colspan" colspan="3">
                    <p>
                        @if (gys.企业基本信息.企业简介 != null && gys.企业基本信息.企业简介 != "")
                                {
                                    if (gys.企业基本信息.企业简介.Length < 200)
                                    {
                                        @gys.企业基本信息.企业简介
                                        <a style="color:#808080" href="~/供应商陈列/Supplier_Destination?id=@gys.Id">...查看详情</a>
                                    }
                                    else
                                    {
                                        @gys.企业基本信息.企业简介.Substring(0, gys.企业基本信息.企业简介.Length - (gys.企业基本信息.企业简介.Length - 200))
                                        <a style="color:#808080;" href="~/供应商陈列/Supplier_Destination?id=@gys.Id">...查看详情</a>
                                    }
                                }
                                else
                                {
                            <span>无企业简介！</span>
                                }
                    </p>
                </td>
            </tr>
        </table>
    </div>*@
