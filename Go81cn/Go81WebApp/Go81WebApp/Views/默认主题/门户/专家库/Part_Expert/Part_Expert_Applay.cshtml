                                        jibie += $(this).attr("id") + ",";
                                    });
                                }

                                thiscount = $("#list").find("input[name='专家类型']:checked").length;
                                if (thiscount != 3 && thiscount != 0) {
                                    $("#list").find("input[name='专家类型']:checked").each(function () {
                                        leixing += $(this).attr("id") + ",";
