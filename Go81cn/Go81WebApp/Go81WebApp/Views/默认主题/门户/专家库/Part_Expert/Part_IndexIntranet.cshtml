                @if (ViewData["登录"].ToString() == "0")
                {
                    <div style="width:450px; margin:0 auto; height:auto; overflow:hidden;">
                        <input type="button" value="申请抽取评审专家" class="gys_ztbsearch_search_botton" style=" height:50px; width:200px; font-size:20px;" onclick="window.location.href = '/登录/Login?ReturnUrl=/专家库'" />
                        <input type="button" value="抽取评审专家" class="gys_ztbsearch_search_botton" style=" height:50px; width:200px; font-size:20px;" onclick="window.location.href = '/登录/Login?ReturnUrl=/专家库'" />
