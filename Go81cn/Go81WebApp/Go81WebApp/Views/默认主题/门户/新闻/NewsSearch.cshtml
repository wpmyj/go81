    
@{
    ViewBag.Title = "新闻搜索";
}
<div class="front_outdiv">
    @if (WebApiApplication.IsIntranet)
    {
        <div class="front_outdiv_right">
            @Html.Action("Part_OtherList", "新闻")
        </div>
        <div class="front_outdiv_left">
            @Html.Action("Part_NewsSearch", "新闻")
        </div>
    }
    else
    {
        <div class="front_outdiv_left">
            @Html.Action("Part_NewsSearch", "新闻")
        </div>
        <div class="front_outdiv_right">
            @Html.Action("Part_OtherList", "新闻")
        </div>
    }
</div>

