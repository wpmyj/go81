        }
    });
    function showTip() {
        var ckbox = document.getElementsByName("sure");
        if (!ckbox[0].checked || !ckbox[1].checked) {
            $("#msgtip").show();
        }
        else {
            $("#pdf3").show();
            $("#msgtip").hide();
        }
    }
    function showDirection(th, str, bl) {
        $(th).show();
        $(str).hide();
        if (bl == true) {
            $("a").each(function () {
                if ($(this).text() == "申请") {
                    $(this).hide();
                }
            });
        }
        else {
            $("a").each(function () {
                if ($(this).text() == "申请") {
                    $(this).show();
                }
            });
        }
    }
    function hidebox(th) {
        $(th).hide();
    }
    var reg = /[^0-9]/g;
    function selectSingle(th) {
        $.get("/jct/ExistName", { n: $(th).val() }, function (data) {
            if (data != "-1") {
                var val = prompt("请选择服务期限", "");
                if (reg.test(val) || val == "") {
                    alert("请输入整数,如123");
                    th.checked = false;
                }
                else {
                    $("#single").val($(th).val() + "," + val);
                }
            }
            else {
                th.checked = false;
            }
        });
    }
    function selectMuptile(th) {
        if (th.checked) {
            $.get("/jct/ExistName", { n: $(th).val() }, function (data) {
                if (data != "-1") {
                    var val = prompt("请选择服务期限", "");
                    if (reg.test(val)||val== "") {
                        alert("请输入整数,如123");
                        th.checked = false;
                    }
                    else {
                        th.lang = val;
                        var str = "";
                        var elem = document.getElementsByName("fee");
                        for (var i = 0; i < elem.length; i++) {
                            if (elem[i].checked) {
                                str += elem[i].value + "," + elem[i].lang + "|";
                            }
                        }
                        $("#multiple").val(str);
                    }
                }
                else {
                    alert("请以供应商的身份登录！");
                    th.checked = false;
                    window.location = "/登录/Login?ReturnUrl=/jct/Applyvip";
                }
            })
        }
        else {
            var str = "";
            var elem = document.getElementsByName("fee");
            for (var i = 0; i < elem.length; i++) {
                if (elem[i].checked) {
                    str += elem[i].value + "," + elem[i].lang + "|";
                }
            }
            $("#multiple").val(str);
        }
    }
    function subBusiness(str) {
        var year = prompt("请输入服务期限");
