                    if (data != "") {
                        alert(data);
                        window.location = "/登录/Login?ReturnUrl=/jct/Applyvip";
                    }
                    else {
                        $("#hidden1Year").val(year);
                        document.forms["business"].submit();
                    }
                });
            }
        }
        else if (str == "basic") {
