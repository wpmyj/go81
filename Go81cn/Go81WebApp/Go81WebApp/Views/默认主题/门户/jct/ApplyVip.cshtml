            if (reg.test(year) || year == "") {
                alert("请输入整数，如3");
            }
            else {
                $.get("/jct/IsExist", { type: "auth" }, function (data) {
                    if (data != "") {
                        alert(data);
                        window.location = "/登录/Login?ReturnUrl=/jct/Applyvip";
                    }
                    else {
                        $("#hiddenYear").val(year);
                        document.forms["auth"].submit();
                    }
                });
