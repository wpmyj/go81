        var ckbox = document.getElementsByName("sure");
        if (!ckbox[0].checked || !ckbox[1].checked) {
            $("#msgtip").show();
        }
        else {
            $("#msgtip").hide();
        }
    };
    $(".action_link").each(function () {
        if ($(this).text() == "军采通") {
            $(this).parent("div").addClass("on");
