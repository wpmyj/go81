                alert("请输入整数，如3");
            }
            else {
                $.get("/jct/IsExist", { type: "basic" }, function (data) {
                    if (data != "") {
                        alert(data);
                        window.location = "/登录/Login?ReturnUrl=/jct/Applyvip";
                    }
                    else {
                        $("#hidden2Year").val(year);
                        document.forms["basic"].submit();
                    }
                });
            }
        }
