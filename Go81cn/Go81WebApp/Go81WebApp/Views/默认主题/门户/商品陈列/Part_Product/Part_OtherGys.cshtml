@{var m = (Dictionary<Go81WebApp.Models.数据模型.用户数据模型.供应商, IEnumerable<MongoDB.Bson.BsonDocument>>)ViewData["供应商匹配商品列表"];}

@if (m.Any())
{
    foreach (var item in m)
    {
        foreach (var i in item.Value)
        {
            <div style="width: 500px; margin: 0 auto; height:25px;">
                <div style="width: 300px; float: left; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; "><a style="text-decoration: underline;" href="/供应商陈列/Supplier_Destination?id=@item.Key.Id" target="_blank">@item.Key.企业基本信息.企业名称@item.Key.企业基本信息.企业名称@item.Key.企业基本信息.企业名称@item.Key.企业基本信息.企业名称</a></div><div style="padding-left: 50px; width: 150px; float: left; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; "><a href="/商品陈列/Product_Detail?id=@(i["_id"].AsInt64)" target="_blank">￥@i["销售信息"]["价格"]</a></div>
            </div>
        }

    }

    @*if(ViewData["currentpage"] != null && ViewData["currentpage"].ToString()!="1")
    {
        <div class="gys_znxx_content_detailbox" style="float: left; width: 918px;">
            @if(int.Parse(ViewData["currentpage"].ToString()) == 1)
            {
                <a>首页</a>
            }
            else
            {
                <a id="1" onclick="changeAdpage(this)">首页</a>
            }
            @for(int i = 0; i <= 10; i++)
            {                       //一共最多显示10个页码，前面5个，后面5个
                if((int.Parse(ViewData["currentpage"].ToString()) + i - 5) >= 1 && (int.Parse(ViewData["currentpage"].ToString()) + i - 5) <= int.Parse(ViewData["pagecount"].ToString()))
                {
                    if(5 == i)
                    {                               //当前页处理
                        <a class="currentPage">@int.Parse(ViewData["currentpage"].ToString())</a>
                    }
                    else
                    {                               //一般页处理
                        <a id="@(int.Parse(ViewData["currentpage"].ToString()) + i - 5)" onclick="changeAdpage(this)">@(int.Parse(ViewData["currentpage"].ToString()) + i - 5)</a>

                    }
                }
            }
            @if(int.Parse(ViewData["currentpage"].ToString()) == int.Parse(ViewData["pagecount"].ToString()))
            {
                <a>末页</a>
            }
            else
            {
                <a id="@int.Parse(ViewData["pagecount"].ToString())" onclick="changeAdpage(this)">末页</a>
            }

        </div>
    }*@
}
else
{
    <div style="color:red;">
        该搜索地区下无供应商提供相同型号的商品
    </div>
}