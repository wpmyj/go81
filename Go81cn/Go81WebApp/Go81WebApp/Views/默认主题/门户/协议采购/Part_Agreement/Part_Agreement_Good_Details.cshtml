                                                else
                                                {
                                                    <span>@list[j - 1].商品信息.品牌 @list[j - 1].商品信息.商品名</span>
                                                }
                                            </td>
                                        }
                                    }
                                    else
                                    {
                                        for (int j = 3 * i + 1; j <= list.Count; j++)
                                        {
                                            <td>
                                                <input type="checkbox" id="@list[j-1].Id" />
                                                @if (list[j - 1].商品信息.商品名.Length > 10)
