                            double s = list.Count / 3;
                            int d = (int)Math.Ceiling(Convert.ToDouble(s.ToString()));
                            for (int i = 0; i <= d; i++)
                            {
                                <tr>
                                    @if (i < d)
                                    {
                                        for (int j = (3 * ((1 / 3) + i)) + 1; j <= 3 * (i + 1); j++)
                                        {
                                            <td>
                                                <input type="checkbox" id="@list[j-1].Id" />
                                                @if (list[j - 1].商品信息.商品名.Length > 10)
