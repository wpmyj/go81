<script type="text/javascript">
    
    function countDown(year, month, day, hour, minute, second, isnum) {
        var now = new Date();
        var endDate = new Date(year, month - 1, day, hour, minute, second);
        var leftTime = endDate.getTime() - now.getTime();//时间差的毫秒数
        var leftsecond = parseInt(leftTime / 1000);
        //var day1=parseInt(leftsecond/(24*60*60*6));
        var day1 = Math.floor(leftsecond / (60 * 60 * 24));//计算出相差天数
        var hour = Math.floor((leftsecond - day1 * 24 * 60 * 60) / 3600);//计算相差小时数
        hour = zeroOccupy(hour);
        var minute = Math.floor((leftsecond - day1 * 24 * 60 * 60 - hour * 3600) / 60);//计算相差分钟数
        minute = zeroOccupy(minute);
        var second = Math.floor(leftsecond - day1 * 24 * 60 * 60 - hour * 3600 - minute * 60);//计算相差秒数
        second = zeroOccupy(second);
        var time = "";
        if (day1 > 0) {
            time = isnum ? hour + ":" + minute + ":" + second : day1 + "天" + hour + "时" + minute + "分" + second + "秒";
        }
        else {
            time = isnum ? hour + ":" + minute + ":" + second : hour + "时" + minute + "分" + second + "秒";
        }
        return time;
    }
    function zeroOccupy(num) {
        var number;
        if (num < 10) {
            number = "0" + num;
        }
        else {
            number = num;
        }
        return number;
    }
    function cSplit(splittime) {
        var date = splittime.split(" ")[0];
        var year = date.split("-")[0];
        var month = date.split("-")[1];
        var day = date.split("-")[2];
        var time = splittime.split(" ")[1];
        var hours = time.split(":")[0];
        var minute = time.split(":")[1];
        var second = time.split(":")[2];
        var arr = new Array(year, month, day, hours, minute, second);
        return arr;
    }


    $(function () {

        var timer1;
        $("li.changechildtab").hover(function () {
            var tab = $(this);
            timer1 = setTimeout(function () {
                var index = tab.attr("index");
                var divs = tab.parents(".outtab").next(".tabcontent");
                tab.addClass("cli").siblings().removeClass("cli");
                divs.children().hide().eq(index).show("fast");
            }, 500);
        }, function () {
            clearTimeout(timer1);
        });

        var listwb = $(".wsjblist");
        $.each(listwb, function (i) {
            var timer = "timer" + i,
                label = $(this).find("label.countdown"),
                endTime = cSplit(label.attr("endtime"));
            timer = setInterval(function () {
                dateTime = countDown(endTime[0], endTime[1], endTime[2], endTime[3], endTime[4], endTime[5], true); //报价结束时间倒计时
                label.html(dateTime);
            }, 1000);
        });


    });
    $(document).ready(function () {
        if ($("ul.showout li").length >=12) {
            setInterval("PicScroll()", 1500);
        }
    });
    function PicScroll() {
        $("ul.showout").animate({ "margin-top": "-20px" },
            function () {
                var s = $("ul.showout li:first").html();
                $("ul.showout li:first").remove();
                $("ul.showout").append("<li  style='background-image:none; padding:0px; border-bottom:none; height:20px; line-height:20px; display:inline-block; text-align:left;'>" + s + "</li>");
                $("ul.showout").css("margin-top", "0px");
            });
    };

    var scrollPic_01 = new ScrollPic();
    scrollPic_01.scrollContId = "gdjyhj"; //内容容器ID
    scrollPic_01.arrLeftId = "gdjyhjl";//左箭头ID
    scrollPic_01.arrRightId = "gdjyhjr"; //右箭头ID
    scrollPic_01.frameWidth = 670;//显示框宽度
    scrollPic_01.pageWidth = 675; //翻页宽度
    scrollPic_01.speed = 40; //移动速度(单位毫秒，越小越快)
    scrollPic_01.space = 40; //每次移动像素(单位px，越大越快)
    scrollPic_01.autoPlay = true; //自动播放
    scrollPic_01.autoPlayTime = 4; //自动播放间隔时间(秒)
    scrollPic_01.initialize(); //初始化


    $(".showBrief").hover(function () {
        var $this = $(this);
        var exist = $(this).attr("exist");
        if (exist != "false") {
            timer = setTimeout(function () {
                $this.next().show().next().show();
            }, 100);
        }
        else {
            timer = setTimeout(function () {
                var id = $this.attr("id");
                var index = $this.attr("index");
                $.ajax({
                    url: '/首页/ShowBrief',
                    data: { id: id },
                    dataType: 'json',
                    beforeSend: function () {
                        var str = "";
                        (index != 5 && index != 10) ? str = "<span class='arrow_left'></span>" : str = "<span class='arrow_right'></span>";
                        (index != 5 && index != 10)
                            ? str += "<div class='gys_brief_info'><img style='width:30px;height:30px;border:none; vertical-align: middle;margin-top:86px;' src='../Images/ajax-loader.gif'/></div>"
                            : str += "<div class='gys_brief_info' style='left:-220px;'><img style='width:30px;height:30px;border:none; vertical-align: middle;margin-top:86px;' src='../Images/ajax-loader.gif'/></div>";
                        $this.after(str);
                    },
                    success: function (data) {
                        var str = "";
                        $this.next().remove();
                        $this.next().remove();
                        (index != 5 && index != 10) ? str = "<span class='arrow_left'></span>" : str = "<span class='arrow_right'></span>";
                        (index != 5 && index != 10) ? str += "<div class='gys_brief_info'>" : str += "<div class='gys_brief_info' style='left:-220px;'>";
                        str += "<img src='" + data.Data.图片 + "' />";
                        str += "<div>主营产品：" + data.Data.主营产品 + "</div>";
                        //str += "<div>信用评级：" + data.Data.认证级别 + "</div>";
                        str += "<div>经营类型：" + data.Data.经营类型 + "</div>";
                        str += "<div>员工人数：" + data.Data.员工人数 + "</div></div>";
                        $this.after(str);
                    },
                    error: function () {
                        var str = "<span class='arrow_left'></span>";
                        str += "<div class='gys_brief_info'>请求出错</div>";
                        $this.after(str);
                    }
                });
                $this.attr({ "exist": "true" });
            }, 100);
        }
    }, function () {
        $(this).next().hide().next().hide();
        clearTimeout(timer);
    });
</script>
