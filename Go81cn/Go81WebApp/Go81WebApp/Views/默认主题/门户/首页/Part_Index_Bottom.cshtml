    <script type="text/javascript">
        function checkYsd() {
            var ysd_num = $("#ysd_num").val();
            var ysd_code = $("#ysd_code").val();
            var ysd_cost = $("#ysd_cost").val();
            if (ysd_num != "" && ysd_code != "" && ysd_cost != "") {

                $.get("/首页/CheckYsd?num=" + encodeURI(ysd_num) + "&code=" + encodeURI(ysd_code) + "&cost=" + encodeURI(ysd_cost), function (response) {

                    $("#checkysdresult").html(response);
                });
            } else {
                $("#checkysdresult").html("请将信息输入完整");
            }

        }
    </script>


