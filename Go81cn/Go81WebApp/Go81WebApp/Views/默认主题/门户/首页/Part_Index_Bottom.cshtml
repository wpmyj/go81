                                    <a class="new" style="width: 175px;" target="_blank" href="~/公告/AnnounceDetail?id=@item.Id" title="@item.内容主体.标题">
                                        @item.内容主体.标题
                                    </a>
                                }
                                else
                                {
                                    @*<span>@item.内容主体.发布时间.ToString("yyyy.MM.dd")</span>*@
                                    <a style="width: 210px; " target="_blank" href="~/公告/AnnounceDetail?id=@item.Id" title="@item.内容主体.标题">
                                        @item.内容主体.标题
                                    </a>
                                }
                            </li>
                        }
                        else
                        {
                            <li class="oddlinecolor">
                                @if (ts.TotalHours < 72)
                                {
                                    <img src="~/Images/newico.gif" />
                                    @*<span>@item.内容主体.发布时间.ToString("yyyy.MM.dd")</span>*@
                                    <a class="new" style="width: 175px; " target="_blank" href="~/公告/AnnounceDetail?id=@item.Id" title="@item.内容主体.标题">@item.内容主体.标题</a>
                                }
                                else
                                {
                                    @*<span>@item.内容主体.发布时间.ToString("yyyy.MM.dd")</span>*@
                                    <a style="width: 210px;" target="_blank" href="~/公告/AnnounceDetail?id=@item.Id" title="@item.内容主体.标题">@item.内容主体.标题</a>
                                }
                            </li>
                        }
                        i++;
                    }
                }
            </ul>
        </div>
    </div>

</div>
