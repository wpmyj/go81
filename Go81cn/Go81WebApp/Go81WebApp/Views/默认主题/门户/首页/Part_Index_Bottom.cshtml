            <ul class="fdiv">
                @{
                    var i = 0;
                    var Message = ViewData["行业动态"] as IEnumerable<公告>;
                }
                @if (Message.Count() == 0)
                {
                    <li class="no_data">
                        <div>无此类信息</div>
                    </li>
                }
                else
                {
                    foreach (var item in (IEnumerable<公告>)ViewData["行业动态"])
                    {
                        TimeSpan ts = DateTime.Now - item.内容主体.发布时间;
                        if (i % 2 == 0)
                        {
                            <li>
                                @if (ts.TotalHours < 72)
                                {
                                    <img src="~/Images/newico.gif" />
