<script type="text/javascript" src="~/JS/SimpleTree.js"></script>
<script type="text/javascript">
    $(function () {
        $(".st_tree").SimpleTree({
            click: function (a) {

                if (!$(a).attr("hasChild")) {
                    window.location.href = $(a).attr("href");
                }
                else if((@ViewData["登录用户类型"].ToString())!="0"){

                    if ($(a).html() == "验收单管理") {
                        $("#indexpagemodalleftmenu0").show();
                    }
                    if ($(a).html() == "我的商品列表") {
                        $("#indexpagemodalleftmenu1").show();
                    }
                }
            }
        });
    });
</script>
