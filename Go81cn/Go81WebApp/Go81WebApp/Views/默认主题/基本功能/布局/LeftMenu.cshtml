@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <div class="st_tree">
        <ul>
            @if (Model != null)
            {
                foreach (var itemlist in Model)
                {
                    <li class="st_tree_first_tittle">
                        <span></span><a href="javascript:void;">@itemlist.Key</a>
                    </li>
                    var itemlistvalue = itemlist.Value;
                    if (itemlistvalue != null)
                    {
                        <ul show="false">
                            @foreach (var item in itemlistvalue as Dictionary<string, object>)
                            {
                                var itemvalue = item.Value;
                                if (itemvalue != null)
                                {
                                    if (itemvalue is string)
                                    {
                                        <li id="@item.Key" class="st_tree_second_title"><span>&nbsp;&nbsp;</span><a href="~/@itemvalue">@item.Key</a></li>
                                    }
                                    else
                                    {
                                        <li id="@item.Key" class="st_tree_second_title"><span>&nbsp;&nbsp;</span><a href="javascript:void;">@item.Key</a></li>
                                            <ul show="false">
                                                @foreach (var it in itemvalue as Dictionary<string,object>)
                                                {
                                                    if (it.Value is string)
                                                    {
                                                        <li id="@it.Key" class="st_tree_third_title"><span>&nbsp;&nbsp;</span><a href="~/@it.Value">@it.Key</a></li>
                                                    }
                                                    else
                                                    {
                                                        <li id="@it.Key" class="st_tree_third_title"><span>&nbsp;&nbsp;</span><a href="javascript:void;">@it.Key</a></li>
                                                            <ul show="false">
                                                                @foreach (var im in it.Value as Dictionary<string, object>)
                                                                {
                                                                    <li id="@im.Key" class="st_tree_third_title"><span>&nbsp;&nbsp;</span><a href="~/@im.Value">@im.Key</a></li>
                                                                }
                                                            </ul>
                                                    }
                                                }
                                            </ul>
                                    }
                                }
                            }
                        </ul>
                    }
                }
            }
            else
            {
                <li>
                    <span>
                        您的账户信息还未经过审核，<br />审核通过后方可使用网站的功能，<br />如有

                        疑问请联系管理员！
                    </span><br />当前状态：@ViewData["单位状态"]
                </li>
            }
