    <script type="text/javascript">
    $("#sh_btn_div input").click(function(){
        var sh_type=$(this).val();
        var openid=$("#openId").val();
        var id=$("#modelId").val();
        if(sh_type=="通过审核"){
            $.ajax({
                url: '/WeChat/DoYsdDetail',
                type:'post',
                data:{id:id,action:sh_type,openid:openid},
                success:function(){
                    //window.location.reload();
                    window.location = "/WeChat/UnitYsdlist?openid=" + openid;
                },
            });
        }
        if(sh_type=="不通过审核"){
            $("#refuse_reason").fadeIn();
        }
    });
    $("#refuse_reason input").click(function(){
        var val=$(this).val();
        var reason_re=$("#reason").val();
        var openid=$("#openId").val();
        var id=$("#modelId").val();
        if(val=="确定"){
            if(reason_re!=""){
                $.ajax({
                    url: '/WeChat/DoYsdDetail',
                    type: 'post',
                    data: { id:id, action: "不通过审核", reason: reason_re, openid: openid },
                    success:function(){
                        //window.location.reload();
                        window.location = "/WeChat/UnitYsdlist?openid=" + openid;
                    },
                });
            }
            else{
                alert("请填写理由！");
            }
        }
        else{
            $("#refuse_reason").fadeOut();
        }
    });
    function showList(){
        $("#good_list").fadeIn();
    };
    $("#modal_clo").click(function(){
        $("#good_list").fadeOut();
    });

    </script>
