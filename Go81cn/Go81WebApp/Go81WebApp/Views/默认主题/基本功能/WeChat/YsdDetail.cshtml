                vl.Add(Tuple.Create(Model.某批物资名称.ToString() + "(点击查看)", (string)null, (string)null, "批", 1.0, price_total, price_total));
            }
            else if (Model.物资服务列表.Count == 0 && Model.验收单附件.Count > 0)
            {
                decimal price_total = Model.验收单附件.Sum(o => o.价格);
                vl.Add(Tuple.Create(Model.某批物资名称.ToString() + "(点击查看)", (string)null, (string)null, "批", 1.0, price_total, price_total));
            }
            else
